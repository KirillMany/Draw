Y2=0
Y3=0
Y4=0

K1=0
K2=0
K3=0
K4=0

e1=0
e2=0
e3=0
e4=0

pos1=0
pos2=0
pos3=0
pos4=0

pro11=0
pro21=0
pro31=0
pro41=0

