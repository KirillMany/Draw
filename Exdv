    for k in range (1,peremichka+1):
        iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
        if sechenie1==2.5 and peremichka1==u'2-х полюс':    
            obj = iFragment.ksFragmentDefinition(box_Peremichka[0], box_Peremichka[0], 0)
        if sechenie1==2.5 and peremichka1==u'3-х полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[1], box_Peremichka[1], 0)
        if sechenie1==2.5 and peremichka1==u'4-х полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[2], box_Peremichka[2], 0)
        if sechenie1==2.5 and peremichka1==u'5-и полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[3], box_Peremichka[3], 0)
        if sechenie1==2.5 and peremichka1==u'10-и полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[4], box_Peremichka[4], 0)
        if sechenie1==2.5 and peremichka1==u'20-и полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[5], box_Peremichka[5], 0)

        if sechenie1==4 and peremichka1==u'2-х полюс':    
            obj = iFragment.ksFragmentDefinition(box_Peremichka[6], box_Peremichka[6], 0)
        if sechenie1==4 and peremichka1==u'3-х полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[7], box_Peremichka[7], 0)
        if sechenie1==4 and peremichka1==u'4-х полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[8], box_Peremichka[8], 0)
        if sechenie1==4 and peremichka1==u'5-и полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[9], box_Peremichka[9], 0)
        if sechenie1==4 and peremichka1==u'10-и полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[10], box_Peremichka[10], 0)
        if sechenie1==4 and peremichka1==u'20-и полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[11], box_Peremichka[11], 0)

        if sechenie1==6 and peremichka1==u'2-х полюс':    
            obj = iFragment.ksFragmentDefinition(box_Peremichka[12], box_Peremichka[12], 0)
        if sechenie1==6 and peremichka1==u'3-х полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[13], box_Peremichka[13], 0)
