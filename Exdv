def ksInsert(x):
    """вставка с разрушением и поворотом на 90градусов"""
    iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
    obj = iFragment.ksFragmentDefinition(x, x, 0)
    iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
    iPlacementParam.Init()
    iPlacementParam.angle = 90
    iPlacementParam.scale_ = 1
    iPlacementParam.xBase = x2
    iPlacementParam.yBase = y2
    iFragment.ksInsertFragmentEx(obj, True, iPlacementParam, 1)


def ksInsert2(x):
    """вставка с разрушением"""
    iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
    obj = iFragment.ksFragmentDefinition(x, x, 0)
    iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
    iPlacementParam.Init()
    iPlacementParam.angle = 0
    iPlacementParam.scale_ = 1
    iPlacementParam.xBase = x2
    iPlacementParam.yBase = y2
    iFragment.ksInsertFragmentEx(obj, True, iPlacementParam, 1)

       
##___________________________________________________________________________выбор корпуса__________________________________________
if xx == u'Вертикально':
    #вставим view в drawing
    insert_view(scale2)

    #вставим корпус с разрушением и сдвинем его в центр
    iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
    iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
    iPlacementParam.Init()
    iPlacementParam.angle = 0.0
    iPlacementParam.scale_ = 1.0
    iPlacementParam.xBase = x2
    iPlacementParam.yBase = y2
    obj=iFragment.ksReadFragmentToGroupEx(link, False, iPlacementParam, True)
    iDocument2D.ksMoveObj(obj,x2,y2)
    iDocument2D.ksEndGroup ()
    iDocument2D.ksStoreTmpGroup(obj)

            
    #добавим боковые виды
    if (kolvo1+kolvo2+kolvo3+kolvo4)>0:
        ksInsert(B)
    if (kolvo5+kolvo6+kolvo7+kolvo8)>0:
        ksInsert(C)
    if (kolvo9+kolvo10+kolvo11+kolvo12+kolvo13+kolvo14+kolvo15+kolvo16)>0 or drenazh == u'да' or drenazh == u'да ТГК':
        ksInsert(D)
    if (kolvo17+kolvo18+kolvo19+kolvo20)>0:
        ksInsert(A)
    
    iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
    iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
    obj=iFragment.ksReadFragmentToGroupEx(size, False, iPlacementParam, True)
    iDocument2D.ksMoveObj(obj,iks,ygrek)
    iDocument2D.ksEndGroup ()
    iDocument2D.ksStoreTmpGroup(obj)
    
        
