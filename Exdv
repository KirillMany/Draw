#словарь для ручек
dict_handle_data1={u'Ручка PS':[box_handle[0],box_handle[5]],
                   u'Ручка PSA':[box_handle[1],box_handle[6]],
                   u'Ручка PSM':[box_handle[3],box_handle[7]],
                   u'Ручка PSS':[box_handle[4],box_proectory1[7]],
                                 }
def insert_elements(a,b,x,p,sh):
    iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
    iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
    iPlacementParam.Init()
    iPlacementParam.angle = 0
    iPlacementParam.scale_ = 1
    iPlacementParam.xBase = 5000+x
    iPlacementParam.yBase = y2-p*40
    iFragment.ksReadFragment(a, True, iPlacementParam)

    iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
    iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
    iPlacementParam.Init()
    iPlacementParam.angle = 0
    iPlacementParam.scale_ = 1
    iPlacementParam.xBase = 5000+x
    iPlacementParam.yBase = y2-500-p*40
    iFragment.ksReadFragment(b, True, iPlacementParam)

    if sh is not None:
        #для вставки шильда поверх элементов
        r="C:/Users/k.itimenev/Desktop/Моя база/моя база/Peppers exd.lfr|5. Шильдики|Шильдик №2 60х20"
        iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
        obj = iFragment.ksFragmentDefinition(r,r,0)
        iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
        iPlacementParam.Init()
        iPlacementParam.angle = 0
        iPlacementParam.scale_ = 1
        iPlacementParam.xBase = 5000+x
        iPlacementParam.yBase = y2+25-p*40
        iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)

        #вставим текст
        iParagraphParam = KAPI.ksParagraphParam(iKompasObject.GetParamStruct(LDefin2D.ko_ParagraphParam))
        iParagraphParam.Init()
        iParagraphParam.x = 5000+x-17
        iParagraphParam.y = y2+25-p*40-6
        iParagraphParam.ang = 0
        iParagraphParam.height = 3
        iParagraphParam.width = 7.2
        iParagraphParam.hFormat = 0
        iParagraphParam.vFormat = 0
        iParagraphParam.style = 1
        iDocument2D.ksParagraph(iParagraphParam)
        iTextLineParam = KAPI.ksTextLineParam(iKompasObject.GetParamStruct(LDefin2D.ko_TextLineParam))
        iTextLineParam.Init()
        iTextLineParam.style = 1
        iTextItemArray = KAPI.ksDynamicArray(iKompasObject.GetDynamicArray(4))
        iTextItemParam = KAPI.ksTextItemParam(iKompasObject.GetParamStruct(LDefin2D.ko_TextItemParam))
        iTextItemParam.Init()
        iTextItemParam.iSNumb = 0
        iTextItemParam.s = sh #текст
        iTextItemParam.type = 0
        iTextItemFontParam = KAPI.ksTextItemFont(iTextItemParam.GetItemFont())
        iTextItemFontParam.Init()
        iTextItemFontParam.bitVector = 4160
        iTextItemFontParam.color = 13684944
        iTextItemFontParam.fontName = "GOST type A"
        iTextItemFontParam.height = 2.5
        iTextItemFontParam.ksu = 0.75
        iTextItemArray.ksAddArrayItem(-1, iTextItemParam)
        iTextLineParam.SetTextItemArr(iTextItemArray)
        iDocument2D.ksTextLine(iTextLineParam)
        obj = iDocument2D.ksEndObj()

        
#пройдемся по словарю для вставки элементов
for k1, v1, in dict_elem_data1.items():
    p=0
    t=0
    for k in range(10):
        p=p+2
        if k1==(sheet2.Cells(114+p,2).value):
            number = int(sheet.Cells(204+p,31).value)
            sh = (sheet.Cells(204+p,26).value)
            for h in range(number):
                insert_elements(v1[0],v1[1],60*h,p,sh)


#пройдемся по словарю для вставки ручек
for k1, v1, in dict_handle_data1.items():
    if k1==handle1:
        c1=v1[0] #для вставки ручки
        d1=v1[1] #для вставки позиции
    if k1==handle2:
        c2=v1[0] #для вставки ручки
        d2=v1[1] #для вставки позиции
    if k1==handle3:
        c3=v1[0] #для вставки ручки
        d3=v1[1] #для вставки позиции

#вставим в чертеж все ручки
#вставим 1ю
for k in range (int(kol_hand1)):
    iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
    obj = iFragment.ksFragmentDefinition(c1,c1,0)
    iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
    iPlacementParam.Init()
    iPlacementParam.angle = 0.0
    iPlacementParam.scale_ = 1.0
    iPlacementParam.xBase = x2+60*k
    iPlacementParam.yBase = y2
    iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
    if sh11 is not None:
        iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
        obj = iFragment.ksFragmentDefinition(r,r,0)
        iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
        iPlacementParam.Init()
        iPlacementParam.angle = 0
        iPlacementParam.scale_ = 1
        iPlacementParam.xBase = x2+60*k
        iPlacementParam.yBase = y2+40
        iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
