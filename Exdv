osob = (sheet.Cells(58,27).value)
#таблица для особых указаний
if osob is not None:
    iViewsAndLayersManager = iKompasDocument2D.ViewsAndLayersManager
    iViews = iViewsAndLayersManager.Views
    iView = iViews.ActiveView
    iSymbols2DContainer = iView._oleobj_.QueryInterface(KAPI7.NamesToIIDMap['ISymbols2DContainer'], pythoncom.IID_IDispatch)
    iSymbols2DContainer = KAPI7.ISymbols2DContainer(iSymbols2DContainer)
    iDrawingTables = iSymbols2DContainer.DrawingTables
    iDrawingTable = iDrawingTables.Add(2, 1, 10, 50, CONST2D7.constants.ksTTLNotCreate)
    iDrawingTable.Angle = 0
    iDrawingTable.FixedCellsSize = False
    iTable = iDrawingTable._oleobj_.QueryInterface(KAPI7.NamesToIIDMap['ITable'], pythoncom.IID_IDispatch)
    iTable = KAPI7.ITable(iTable)
    iTableCell = iTable.Cell(0, 0)
    iCellFormat = iTableCell._oleobj_.QueryInterface(KAPI7.NamesToIIDMap['ICellFormat'], pythoncom.IID_IDispatch)
    iCellFormat = KAPI7.ICellFormat(iCellFormat)
    iCellFormat.OneLine = False
    iCellFormat.HFormat = CONST2D7.constants.ksHFormatStrNarrowing
    iCellFormat.VFormat = True
    iCellFormat.LeftEdge = 0.5
    iCellFormat.RightEdge = -0.5
    iCellFormat.SpaceAfter = 0
    iCellFormat.SpaceBefore = 0
    iCellFormat.ReadOnly = False
    iCellFormat.TextStyle = 9
    iCellFormat.Height = 10
    iCellFormat.Width = 50
    iTextObject = iTableCell.Text
    iText = iTextObject._oleobj_.QueryInterface(KAPI7.NamesToIIDMap['IText'], pythoncom.IID_IDispatch)
    iText = KAPI7.IText(iText)
    iText.Clear()
    iTextLine = iText.Add()
    iTextLine.Align = CONST2D7.constants.ksAlignCenter
    iTextLine.IndentedLine = 0
    iTextLine.LeftEdge = 0
    iTextLine.Level = 0
    iTextLine.Numbering = 0
    iTextLine.RightEdge = 0
    iTextLine.StepAfterParagraph = 0
    iTextLine.StepBeforeParagraph = 0
    iTextLine.Step = 1
    iTextLine.Style = 9
    iTextItem = iTextLine.Add()
    iTextItem.ItemType = CONST2D7.constants.ksTItString
    iTextItem.NewLine = 4096
    iTextItem.Str = 'Особые указания'
    iTextItem.SymbolFontName = ''
    iTextFont = iTextItem._oleobj_.QueryInterface(KAPI7.NamesToIIDMap['ITextFont'], pythoncom.IID_IDispatch)
    iTextFont = KAPI7.ITextFont(iTextFont)
    iTextFont.Bold = False
    iTextFont.Color = 0
    iTextFont.FontName = 'GOST type A'
    iTextFont.Height = 3.5
    iTextFont.Italic = True
    iTextFont.Underline = False
    iTextFont.WidthFactor = 1
    iTextItem.Update()
    iCellBoundaries = iTableCell._oleobj_.QueryInterface(KAPI7.NamesToIIDMap['ICellBoundaries'], pythoncom.IID_IDispatch)
    iCellBoundaries = KAPI7.ICellBoundaries(iCellBoundaries)
    iCellBoundaries.SetLineStyle(CONST2D7.constants.ksCBLeftBorder, 1)
    iCellBoundaries.SetLineStyle(CONST2D7.constants.ksCBRightBorder, 1)
    iCellBoundaries.SetLineStyle(CONST2D7.constants.ksCBTopBorder, 1)
    iCellBoundaries.SetLineStyle(CONST2D7.constants.ksCBBottomBorder, 1)
    iCellBoundaries.SetLineVisible(CONST2D7.constants.ksCBLeftBorder, 1)
    iCellBoundaries.SetLineVisible(CONST2D7.constants.ksCBRightBorder, 1)
    iCellBoundaries.SetLineVisible(CONST2D7.constants.ksCBTopBorder, 1)
    iCellBoundaries.SetLineVisible(CONST2D7.constants.ksCBBottomBorder, 1)
    iTableCell = iTable.Cell(1, 0)
    iCellFormat = iTableCell._oleobj_.QueryInterface(KAPI7.NamesToIIDMap['ICellFormat'], pythoncom.IID_IDispatch)
    iCellFormat = KAPI7.ICellFormat(iCellFormat)
    iCellFormat.OneLine = False
    iCellFormat.HFormat = CONST2D7.constants.ksHFormatStrNarrowing
    iCellFormat.VFormat = True
    iCellFormat.LeftEdge = 0.5
    iCellFormat.RightEdge = -0.5
    iCellFormat.SpaceAfter = 0
    iCellFormat.SpaceBefore = 0
    iCellFormat.ReadOnly = False
    iCellFormat.TextStyle = 10
    iCellFormat.Height = 30
    iTextObject = iTableCell.Text
    iText = iTextObject._oleobj_.QueryInterface(KAPI7.NamesToIIDMap['IText'], pythoncom.IID_IDispatch)
    iText = KAPI7.IText(iText)
    iText.Clear()
    iTextLine = iText.Add()
    iTextLine.Align = CONST2D7.constants.ksAlignCenter
    iTextLine.IndentedLine = 0
    iTextLine.LeftEdge = 0
    iTextLine.Level = 0
    iTextLine.Numbering = 0
    iTextLine.RightEdge = 0
    iTextLine.StepAfterParagraph = 0
    iTextLine.StepBeforeParagraph = 0
    iTextLine.Step = 7
    iTextLine.Style = 10
    iTextItem = iTextLine.Add()
    iTextItem.ItemType = CONST2D7.constants.ksTItString
    iTextItem.NewLine = 4096
    iTextItem.Str = osob
    #вставка текста особых указаний ___________________________________________________
    iTextItem.SymbolFontName = ''
    iTextFont = iTextItem._oleobj_.QueryInterface(KAPI7.NamesToIIDMap['ITextFont'], pythoncom.IID_IDispatch)
    iTextFont = KAPI7.ITextFont(iTextFont)
    iTextFont.Bold = False
    iTextFont.Color = 0
    iTextFont.FontName = 'GOST type A'
    iTextFont.Height = 5
    iTextFont.Italic = True
    iTextFont.Underline = False
    iTextFont.WidthFactor = 1
    iTextItem.Update()
    iCellBoundaries = iTableCell._oleobj_.QueryInterface(KAPI7.NamesToIIDMap['ICellBoundaries'], pythoncom.IID_IDispatch)
    iCellBoundaries = KAPI7.ICellBoundaries(iCellBoundaries)
    iCellBoundaries.SetLineStyle(CONST2D7.constants.ksCBLeftBorder, 1)
    iCellBoundaries.SetLineStyle(CONST2D7.constants.ksCBRightBorder, 1)
    iCellBoundaries.SetLineStyle(CONST2D7.constants.ksCBTopBorder, 1)
    iCellBoundaries.SetLineStyle(CONST2D7.constants.ksCBBottomBorder, 1)
    iCellBoundaries.SetLineVisible(CONST2D7.constants.ksCBLeftBorder, 1)
    iCellBoundaries.SetLineVisible(CONST2D7.constants.ksCBRightBorder, 1)
    iCellBoundaries.SetLineVisible(CONST2D7.constants.ksCBTopBorder, 1)
    iCellBoundaries.SetLineVisible(CONST2D7.constants.ksCBBottomBorder, 1)
    iDrawingTable.X = 20
    iDrawingTable.Y = 278
    iDrawingTable.Update()


