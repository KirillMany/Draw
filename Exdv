                iPlacementParam.yBase = koordinat4[-1+i]
                iPlacementParam.xBase = x2
                iFragment.ksReadFragment(pos, True, iPlacementParam)
                

    Vvod_input(i=0,kolvo=kolvo17,a=a17,pro1=pro171,pro2=pro52,pos=pos17,et=et17,temp=sp17)
    Vvod_input(i=kolvo17,kolvo=kolvo18,a=a18,pro1=pro181,pro2=pro62,pos=pos18,et=et18,temp=sp18)
    Vvod_input(i=kolvo17+kolvo18,kolvo=kolvo19,a=a19,pro1=pro191,pro2=pro72,pos=pos19,et=et19,temp=sp19)
    Vvod_input(i=kolvo17+kolvo18+kolvo19,kolvo=kolvo20,a=a20,pro1=pro201,pro2=pro82,pos=pos20,et=et20,temp=sp20)
    
if dL<=0:
    #расчет координат
    if e17==2:
        kk17=kk17*1.2
    if e18==2:
        kk18=kk18*1.2
    if e19==2:
        kk19=kk19*1.2
    if e20==2:
        kk20=kk20*1.2
    
    dlina1=kk17*kolvo17+kk18*kolvo18+kk19*kolvo19+kk20*kolvo20
    dlina=dlina*0.9
    
    dL=dlina-dlina1
    summa=kolvo17+kolvo18+kolvo19+kolvo20#сумма всех вводов
    dxx=dL/(summa+1)#отсуп
    koordinat4=[]
        
    if kolvo17==0:
        kk17=kk18      
    if kolvo18==0:
        kk18=kk17
    if kolvo19==0:
        kk19=kk18
    if kolvo20==0:
        kk20=kk19
    xx1=x2-dlina/2+dxx+kk17/2#расчет первой координаты    
    koordinat4.append(xx1)
    i=0
    for k in range(kolvo17-1):
        i=i+1
        xx2=koordinat4[-1+i]+kk17+dxx
        koordinat4.append(xx2)
    for k in range(kolvo18):
        i=i+1
        if k==0:
            xx3=koordinat4[-1+i]+kk17/2+kk18/2+dxx #при первом вводе
        else:
            xx3=koordinat4[-1+i]+kk18+dxx
        koordinat4.append(xx3)
    for k in range(kolvo19):
        i=i+1
        if k==0:
            xx4=koordinat4[-1+i]+kk18/2+kk19/2+dxx #при первом вводе
        else:
            xx4=koordinat4[-1+i]+kk19+dxx
        koordinat4.append(xx4)
    for k in range(kolvo20):
        i=i+1
        if k==0:
            xx5=koordinat4[-1+i]+kk19/2+kk20/2+dxx #при первом вводе
        else:
            xx5=koordinat4[-1+i]+kk20+dxx
        koordinat4.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat4)):
        if koordinat4[x]<x2:
            koordinat4[x]=koordinat4[x]
        else:
            koordinat4[x]=koordinat4[x]
            
    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et,Y,temp):
        for k in range(kolvo):
            #ввод
            i=i+1
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0.0
            iPlacementParam.scale_ = 1.0
            iPlacementParam.yBase = koordinat4[-1+i]
            if i%2==0:
                iPlacementParam.xBase = x2+Y
            if i%2==1:
                iPlacementParam.xBase = x2-Y
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if sp==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(temp,temp,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 45
                iPlacementParam.scale_ = 1
                iPlacementParam.yBase = koordinat4[-1+i]
                if i%2==0:
                    iPlacementParam.xBase = x2+Y
                if i%2==1:
                    iPlacementParam.xBase = x2-Y
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k==0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.yBase = koordinat4[-1+i]
                if i%2==0:
                    iPlacementParam.xBase = x2+Y
                if i%2==1:
                    iPlacementParam.xBase = x2-Y
                iFragment.ksReadFragment(pos, True, iPlacementParam)
                

    Vvod_input(i=0,kolvo=kolvo17,a=a17,pro1=pro171,pro2=pro52,pos=pos17,et=et17,Y=Y17,temp=sp17)
    Vvod_input(i=kolvo17,kolvo=kolvo18,a=a18,pro1=pro181,pro2=pro62,pos=pos18,et=et18,Y=Y18,temp=sp18)
    Vvod_input(i=kolvo17+kolvo18,kolvo=kolvo19,a=a19,pro1=pro191,pro2=pro72,pos=pos19,et=et19,Y=Y19,temp=sp19)
    Vvod_input(i=kolvo17+kolvo18+kolvo19,kolvo=kolvo20,a=a20,pro1=pro201,pro2=pro82,pos=pos20,et=et20,Y=Y20,temp=sp20)


#ЗИП
zp=sheet.Cells(70,65).value
#ввод ЗИП№1
zpkolvo1=sheet.Cells(59,25).value
#кол-во ЗИП№1
zp1=sheet.Cells(71,65).value
#ввод ЗИП№2
zpkolvo2=sheet.Cells(61,25).value
#кол-во ЗИП№2
zp2=sheet.Cells(72,65).value
#ввод ЗИП№3
zpkolvo3=sheet.Cells(63,25).value
#кол-во ЗИП№3

if zpkolvo1==None:
    zpkolvo1=0
if zpkolvo2==None:
    zpkolvo2=0
if zpkolvo3==None:
    zpkolvo3=0

