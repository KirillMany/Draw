sumvvodov = kolvo1+kolvo2+kolvo3+kolvo4+kolvo5+kolvo6+kolvo7+kolvo8+kolvo9+kolvo10+kolvo11+kolvo12+kolvo13+kolvo14+kolvo15+kolvo16+kolvo17+kolvo18+kolvo19+kolvo20

def insert_zb4(sumvvodov,x2,vert):
    a = "C:/Users/k.itimenev/Desktop/Моя база/моя база/Peppers exd.lfr|4. Клеммы|Weidmuller|ZB 4|ZB-4"
    for k in range(sumvvodov):
        iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
        obj = iFragment.ksFragmentDefinition(a, a, 0)
        iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
        iPlacementParam.Init()
        iPlacementParam.angle = 0
        iPlacementParam.scale_ = 1
        iPlacementParam.xBase = k*7+x2-7-3.5*sumvvodov
        if (kolvo1+kolvo2+kolvo3+kolvo4)>0 and (kolvo1+kolvo2+kolvo3+kolvo4)>(kolvo9+kolvo10+kolvo11+kolvo12+kolvo13+kolvo14+kolvo15+kolvo16):
            iPlacementParam.yBase = y2+vert
        else:
            iPlacementParam.yBase = y2-vert
        iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)


def insert_zb4pos(x2,vert):
    #обозначение позиции 
    iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
    iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
    iPlacementParam.Init()
    iPlacementParam.angle = 0
    iPlacementParam.scale_ = 1
    iPlacementParam.xBase = x2
    if (kolvo1+kolvo2+kolvo3+kolvo4)>0 and (kolvo1+kolvo2+kolvo3+kolvo4)>(kolvo9+kolvo10+kolvo11+kolvo12+kolvo13+kolvo14+kolvo15+kolvo16):
        iPlacementParam.yBase = y2+vert
    else:
        iPlacementParam.yBase = y2-vert
