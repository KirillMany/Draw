for k in range(20):
    if (sheet.Cells(70+k,64).value) == -2146826246:
        (sheet.Cells(70+k,64).value) = 0
    
for k in range(4):
    if (sheet.Cells(33+k,25).value) is None:
        (sheet.Cells(33+k,25).value) = 0
    
