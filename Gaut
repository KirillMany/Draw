    iViews = iViewsAndLayersManager.Views
    iView = iViews.ActiveView
    iSymbols2DContainer = iView._oleobj_.QueryInterface(KAPI7.NamesToIIDMap['ISymbols2DContainer'], pythoncom.IID_IDispatch)
    iSymbols2DContainer = KAPI7.ISymbols2DContainer(iSymbols2DContainer)
    iDrawingTables = iSymbols2DContainer.DrawingTables
    iDrawingTable = iDrawingTables.Add(2, 1, 10, 50, CONST2D7.constants.ksTTLNotCreate)
    iDrawingTable.Angle = 0.00000000000
    iDrawingTable.FixedCellsSize = False
    iTable = iDrawingTable._oleobj_.QueryInterface(KAPI7.NamesToIIDMap['ITable'], pythoncom.IID_IDispatch)
    iTable = KAPI7.ITable(iTable)
    iTableCell = iTable.Cell(0, 0)
    iCellFormat = iTableCell._oleobj_.QueryInterface(KAPI7.NamesToIIDMap['ICellFormat'], pythoncom.IID_IDispatch)
    iCellFormat = KAPI7.ICellFormat(iCellFormat)
    iCellFormat.OneLine = False
    iCellFormat.HFormat = CONST2D7.constants.ksHFormatStrNarrowing
    iCellFormat.VFormat = True
    iCellFormat.LeftEdge = 0.50000000000
    iCellFormat.RightEdge = -0.50000000000
    iCellFormat.SpaceAfter = 0.00000000000
    iCellFormat.SpaceBefore = 0.00000000000
    iCellFormat.ReadOnly = False
    iCellFormat.TextStyle = 9
    iCellFormat.Height = 10.00000000000
    iCellFormat.Width = 50.00000000000
    iTextObject = iTableCell.Text
    iText = iTextObject._oleobj_.QueryInterface(KAPI7.NamesToIIDMap['IText'], pythoncom.IID_IDispatch)
    iText = KAPI7.IText(iText)
    iText.Clear()
    iTextLine = iText.Add()
    iTextLine.Align = CONST2D7.constants.ksAlignCenter
    iTextLine.IndentedLine = 0.00000000000
    iTextLine.LeftEdge = 0.00000000000
    iTextLine.Level = 0
    iTextLine.Numbering = 0
    iTextLine.RightEdge = 0.00000000000
    iTextLine.StepAfterParagraph = 0.00000000000
    iTextLine.StepBeforeParagraph = 0.00000000000
    iTextLine.Step = 1.00000000000
    iTextLine.Style = 9
    iTextItem = iTextLine.Add()
    iTextItem.ItemType = CONST2D7.constants.ksTItString
    iTextItem.NewLine = 4096
    iTextItem.Str = 'Особые указания'
    iTextItem.SymbolFontName = ''
