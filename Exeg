iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
iPlacementParam.Init()
iPlacementParam.angle = 0
iPlacementParam.scale_ = 1
iPlacementParam.xBase = x2
iPlacementParam.yBase = y2


def insert_korpus(x):
    obj = iFragment.ksReadFragmentToGroupEx(x, False, iPlacementParam, True)
    iDocument2D.ksMoveObj(obj,x2,y2)
    iDocument2D.ksEndGroup ()
    iDocument2D.ksStoreTmpGroup(obj)

    
if x1 == u'TM PW06' or x1 == u'BM PW06'or x1 == u'TM PW06PP' or x1 == u'BM PW06PP'or x1 == u'TM PW07' or x1 == u'BM PW07'or x1 == u'TM PW07PP' or x1 == u'BM PW07PP'or x1 == u'TM AW07' or x1 == u'BM AW07'or x1 == u'TM A112' or x1 == u'BM A112'or x1 == u'TM AW08' or x1 == u'BM AW08':
    if option5 == u'да':
        if (kolvo1+kolvo2+kolvo3+kolvo4)>0 and (kolvo1+kolvo2+kolvo3+kolvo4)>(kolvo9+kolvo10+kolvo11+kolvo12+kolvo13+kolvo14+kolvo15+kolvo16):
            insert_korpus(insert1)
        else:
            insert_korpus(insert2)
    if option5 == u'нет':
        insert_korpus(insert)
else:
    insert_korpus(insert)

       
