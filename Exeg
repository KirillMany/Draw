            Vvod_input(i=0,kolvo=kolvo99,a=a9,pro1=pro91,pro2=pro92,pos=pos9,et=et9,temp=sp9,kk=-kk9)
            Vvod_input2(i=0,kolvo=kolvo999,a=a9,pro1=pro91,pro2=pro92,pos=pos9,et=et9,temp=sp9,kk=kk9)

