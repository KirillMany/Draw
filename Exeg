        koordinat3.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat3)):
        if koordinat3[x]<x2:
            koordinat3[x]=koordinat3[x]-ror
        else:
            koordinat3[x]=koordinat3[x]+ror
    
    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et,Y,temp):
        for k in range(kolvo):
            #ввод
            i=i+1
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0
            iPlacementParam.scale_ = 1
            iPlacementParam.xBase = koordinat3[-1+i]
            if i%2==0:
                iPlacementParam.yBase = y2+Y
            if i%2==1:
                iPlacementParam.yBase = y2-Y
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 180
                iPlacementParam.scale_ = 1
                iPlacementParam.xBase = koordinat3[-1+i]
                if i%2==0:
                    iPlacementParam.yBase = y2+Y
                if i%2==1:
                    iPlacementParam.yBase = y2-Y
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if sp==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(temp,temp,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 180
                iPlacementParam.scale_ = 1
                iPlacementParam.xBase = koordinat3[-1+i]
                if i%2==0:
