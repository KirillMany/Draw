def InsertZps(v):
    iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
    obj = iFragment.ksFragmentDefinition(v, v, 0)
    iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
    iPlacementParam.Init()
    iPlacementParam.angle = 1
    iPlacementParam.scale_ = 1
    iPlacementParam.xBase = 15
    iPlacementParam.yBase = 15
    iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)

    
#пройдемся по словарю
for k, v in dict_insert_zip.items():
    for t in range(23):
        if k == sheet3.Cells(32+t,43).value:
            for i in range (sheet3.Cells(32+t,11)):
                InsertZps(v)


#создадим папку с названием файла
#определим ячейки для ввода пути к папке
a = u'U:\\Peppers\\10_Проектирование\\~АрхивЧертежей~\\Архив УУМ'
#основной путь
sheet4 = app.ActiveWorkbook.Sheets(u'Главная')

b = (sheet4.Cells(47,67).value)
#серия W или 1
c = (sheet4.Cells(48,67).value)
#серия TM или ВМ
d = (sheet4.Cells(46,67).value)
#материал полиэстер или алюминий
e = (sheet4.Cells(44,67).value)
#номер корпуса
ee = u'\\'+c+u' '+d+b+e
#определим крайнюю папку
#перед закрытием поменяем обратно корпус АР
if sheet.Cells(66,25).value == u'260 x 160 x 90':
    sheet.Cells(66,25).value = u'260 x 160 x 90AP'
app.ActiveWindow.Close(True)
app.Quit()
#опишем условия создания папок
if b == u'W':
    b = u'\\Серия W'
if b == u'1':
    b = u'\\Серия 100'
if c == u'TM':
    c = u'\\TM'
    if d == u'P':
        d = u'\\TM P'
    if d == u'A':
        d = u'\\TM A'
    if d == u'S':
        d = u'\\TM S'
if c == u'CM':
    c = u'\\CM'
    if d == u'P':
        d=u'\\CM P'
    if d == u'A':
        d = u'\\CM A'
if c == u'BM':
    c = u'\\BM'
    if d == u'P':
        d = u'\\BM P'
    if d == u'A':
        d = u'\\BM A'
    if d == u'S':
        d = u'\\BM S'


#имя файла
g = a+b+c+d+ee+u'\\'+nameis
#генерация полного пути


try:
    os.mkdir(g)
    #создание папки
except WindowsError:
    application.HideMessage = kompas6_constants.ksHideMessageNo
    tkMessageBox.showinfo(title = u"Прошу проверить!", message = u"Такая папка уже есть!")
    sys.exit()



    

