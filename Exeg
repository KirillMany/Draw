for k, v in dict_korpus_data4.items(): 
    if k == x1:
        dlina = v[0]
        shir = v[1]
        x2 = v[2]
        y2 = v[3]
        y22 = v[4]

        kk00 = v[5]
        kk22 = v[6]
        kk33 = v[7]
        kk44 = v[8]
        kk55 = v[9]
        kk66 = v[10]
        kk77 = v[11]

        yy0 = v[12]
        yy2 = v[13]
        yy3 = v[14]
        yy4 = v[15]
        yy5 = v[16]
        yy6 = v[17]
        yy7 = v[18]


a17=0
a18=0
a19=0
a20=0

dx17=0
dx18=0
dx19=0
dx20=0

kk17=0
kk18=0
kk19=0
kk20=0

Y17=0
Y18=0
Y19=0
Y20=0

K17=0
K18=0
K19=0
K20=0

e17=0
e18=0
e19=0
e20=0

pos17=0
pos18=0
pos19=0
pos20=0

pro171=0
pro181=0
pro191=0
pro201=0

pro52=0
pro62=0
pro72=0
pro82=0

et17=0
et18=0
et19=0
et20=0

sp17=0
sp18=0
sp19=0
sp20=0

ror = 0
if x1 == u'TM A139'or x1 == u'BM A139':
    ror = 28

for K17, v17, in dict_vvod_data1.items():
    if K17==vvod17:
        a17=v17[0]
        dx17=v17[1]
        kk17=v17[2]
        Y17=v17[4]
        K17=v17[3]
        e17=v17[5]
        pos17=v17[6]
        pro171=v17[7]
        pro52=v17[8]
        et17=v17[9]
        sp17=v17[10]
    if K17==vvod18:
        a18=v17[0]
        dx18=v17[1]
        kk18=v17[2]
        Y18=v17[4]
        K18=v17[3]
        e18=v17[5]
        pos18=v17[6]
        pro181=v17[7]
        pro62=v17[8]
        et18=v17[9]
        sp18=v17[10]
    if K17==vvod19:
        a19=v17[0]
        dx19=v17[1]
        kk19=v17[2]
        Y19=v17[4]
        K19=v17[3]
        e19=v17[5]
        pos19=v17[6]
        pro191=v17[7]
        pro72=v17[8]
        et19=v17[9]
        sp19=v17[10]
    if K17==vvod20:
        a20=v17[0]
        dx20=v17[1]
        kk20=v17[2]
        Y20=v17[4]
        K20=v17[3]
        e20=v17[5]
        pos20=v17[6]
        pro201=v17[7]
        pro82=v17[8]
        et20=v17[9]
        sp20=v17[10]


dlina1=kk17*kolvo17+kk18*kolvo18+kk19*kolvo19+kk20*kolvo20
dL=dlina-dlina1
Ld = dlina1/dlina
summa=kolvo17+kolvo18+kolvo19+kolvo20#сумма всех вводов
dxx=dL/(summa+1)#отсуп

#расстановка в 1ряд
if dL > 0 and (Ld < 1):
    koordinat4=[]

    if kolvo17==0:
        kk17=kk18      
    if kolvo18==0:
        kk18=kk17
    if kolvo19==0:
        kk19=kk18
    if kolvo20==0:
        kk20=kk19
    xx1=y2-dlina/2+dxx+kk17/2#расчет первой координаты    
    koordinat4.append(xx1)
    i=0
    for k in range(kolvo17-1):
        i=i+1
        xx2=koordinat4[-1+i]+kk17+dxx
        koordinat4.append(xx2)
    for k in range(kolvo18):
        i=i+1
        if k==0:
            xx3=koordinat4[-1+i]+kk17/2+kk18/2+dxx #при первом вводе
        else:
            xx3=koordinat4[-1+i]+kk18+dxx
        koordinat4.append(xx3)
    for k in range(kolvo19):
        i=i+1
        if k==0:
            xx4=koordinat4[-1+i]+kk18/2+kk19/2+dxx #при первом вводе
        else:
            xx4=koordinat4[-1+i]+kk19+dxx
        koordinat4.append(xx4)
    for k in range(kolvo20):
        i=i+1
        if k==0:
            xx5=koordinat4[-1+i]+kk19/2+kk20/2+dxx #при первом вводе
        else:
            xx5=koordinat4[-1+i]+kk20+dxx
        koordinat4.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat4)):
        if koordinat4[x]<x2:
            koordinat4[x]=koordinat4[x]-ror
        else:
            koordinat4[x]=koordinat4[x]+ror

    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et,temp):
        for k in range(kolvo):
            i=i+1
            #ввод
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0
            iPlacementParam.scale_ = 1
            iPlacementParam.yBase = koordinat4[-1+i]
            iPlacementParam.xBase = x2
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 45
                iPlacementParam.scale_ = 1
                iPlacementParam.yBase = koordinat4[-1+i]
                iPlacementParam.xBase = x2
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if sp==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(temp,temp,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 45
                iPlacementParam.scale_ = 1
                iPlacementParam.yBase = koordinat4[-1+i]
                iPlacementParam.xBase = x2
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #проекции
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            if shir==4:
                obj = iFragment.ksFragmentDefinition(pro1,pro1,0)
            if shir==6:
                obj = iFragment.ksFragmentDefinition(pro2,pro2,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 90
            iPlacementParam.scale_ = 1
            iPlacementParam.yBase = koordinat4[-1+i]
            iPlacementParam.xBase = x2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k==0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0
                iPlacementParam.scale_ = 1
                iPlacementParam.yBase = koordinat4[-1+i]
                iPlacementParam.xBase = x2
                iFragment.ksReadFragment(pos, True, iPlacementParam)
                

    Vvod_input(i=0,kolvo=kolvo17,a=a17,pro1=pro171,pro2=pro52,pos=pos17,et=et17,temp=sp17)
    Vvod_input(i=kolvo17,kolvo=kolvo18,a=a18,pro1=pro181,pro2=pro62,pos=pos18,et=et18,temp=sp18)
    Vvod_input(i=kolvo17+kolvo18,kolvo=kolvo19,a=a19,pro1=pro191,pro2=pro72,pos=pos19,et=et19,temp=sp19)
    Vvod_input(i=kolvo17+kolvo18+kolvo19,kolvo=kolvo20,a=a20,pro1=pro201,pro2=pro82,pos=pos20,et=et20,temp=sp20)
    
#расстановка в 2ряда
if dL <= 0 and (1 <= Ld <= 1.8):
    #расчет координат
    if e17==2:
        kk17=kk17*1.2
    if e18==2:
        kk18=kk18*1.2
    if e19==2:
        kk19=kk19*1.2
    if e20==2:
        kk20=kk20*1.2
    
    dlina1=kk17*kolvo17+kk18*kolvo18+kk19*kolvo19+kk20*kolvo20
    dlina=dlina*0.8
    
    dL=dlina-dlina1
    summa=kolvo17+kolvo18+kolvo19+kolvo20#сумма всех вводов
    dxx=dL/(summa+1)#отсуп
    koordinat4=[]
        
    if kolvo17==0:
        kk17=kk18      
    if kolvo18==0:
        kk18=kk17
    if kolvo19==0:
        kk19=kk18
    if kolvo20==0:
        kk20=kk19
    xx1=y2-dlina/2+dxx+kk17/2#расчет первой координаты    
    koordinat4.append(xx1)
    i=0
    for k in range(kolvo17-1):
        i=i+1
        xx2=koordinat4[-1+i]+kk17+dxx
        koordinat4.append(xx2)
    for k in range(kolvo18):
        i=i+1
        if k==0:
            xx3=koordinat4[-1+i]+kk17/2+kk18/2+dxx #при первом вводе
        else:
            xx3=koordinat4[-1+i]+kk18+dxx
        koordinat4.append(xx3)
    for k in range(kolvo19):
        i=i+1
        if k==0:
            xx4=koordinat4[-1+i]+kk18/2+kk19/2+dxx #при первом вводе
        else:
            xx4=koordinat4[-1+i]+kk19+dxx
        koordinat4.append(xx4)
    for k in range(kolvo20):
        i=i+1
        if k==0:
            xx5=koordinat4[-1+i]+kk19/2+kk20/2+dxx #при первом вводе
        else:
            xx5=koordinat4[-1+i]+kk20+dxx
        koordinat4.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat4)):
        if koordinat4[x]<x2:
            koordinat4[x]=koordinat4[x]-ror
        else:
            koordinat4[x]=koordinat4[x]+ror
            
    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et,Y,temp):
        for k in range(kolvo):
            #ввод
            i=i+1
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0
            iPlacementParam.scale_ = 1
            iPlacementParam.yBase = koordinat4[-1+i]
            if i%2==0:
                iPlacementParam.xBase = x2+Y
            if i%2==1:
                iPlacementParam.xBase = x2-Y
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 45
                iPlacementParam.scale_ = 1
                iPlacementParam.yBase = koordinat4[-1+i]
                if i%2==0:
                    iPlacementParam.xBase = x2+Y
                if i%2==1:
                    iPlacementParam.xBase = x2-Y
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if sp==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(temp,temp,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 45
                iPlacementParam.scale_ = 1
                iPlacementParam.yBase = koordinat4[-1+i]
                if i%2==0:
                    iPlacementParam.xBase = x2+Y
                if i%2==1:
                    iPlacementParam.xBase = x2-Y
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #проекции
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            if shir==4:
                obj = iFragment.ksFragmentDefinition(pro1,pro1,0)
            if shir==6:
                obj = iFragment.ksFragmentDefinition(pro2,pro2,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 90
            iPlacementParam.scale_ = 1
            iPlacementParam.yBase = koordinat4[-1+i]
            iPlacementParam.xBase = x2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k==0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0
                iPlacementParam.scale_ = 1
                iPlacementParam.yBase = koordinat4[-1+i]
                if i%2==0:
                    iPlacementParam.xBase = x2+Y
                if i%2==1:
                    iPlacementParam.xBase = x2-Y
                iFragment.ksReadFragment(pos, True, iPlacementParam)
                

    Vvod_input(i=0,kolvo=kolvo17,a=a17,pro1=pro171,pro2=pro52,pos=pos17,et=et17,Y=Y17,temp=sp17)
    Vvod_input(i=kolvo17,kolvo=kolvo18,a=a18,pro1=pro181,pro2=pro62,pos=pos18,et=et18,Y=Y18,temp=sp18)
