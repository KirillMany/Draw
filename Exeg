a1 = 0
a2 = 0
a3 = 0
a4 = 0

dx1 = 0
dx2 = 0
dx3 = 0
dx4 = 0

kk1 = 0
kk2 = 0
kk3 = 0
kk4 = 0

Y1 = 0
Y2 = 0
Y3 = 0
Y4 = 0

K1 = 0
K2 = 0
K3 = 0
K4 = 0

e1 = 0
e2 = 0
e3 = 0
e4 = 0

pos1 = 0
pos2 = 0
pos3 = 0
pos4 = 0 

pro11 = 0
pro21 = 0
pro31 = 0
pro41 = 0

pro12 = 0
pro22 = 0
pro32 = 0
pro42 = 0

et1 = 0
et2 = 0
et3 = 0
et4 = 0

sp1 = 0
sp2 = 0
sp3 = 0
sp4 = 0

ror = 0
if x1 == u'TM PW11' or x1 == u'TM PW16' or x1 == u'TM A121' or x1 == u'TM A132'or x1 == u'TM A137'or x1 == u'TM A138'or x1 == u'TM A139'or x1 == u'BM PW11' or x1 == u'BM PW16' or x1 == u'BM A121' or x1 == u'BM A132'or x1 == u'BM A137'or x1 == u'BM A138'or x1 == u'BM A139':
    ror = 28

for k1, v1, in dict_vvod_data1.items():
    if k1 == vvod1:
        a1 = v1[0]
        dx1 = v1[1]
        kk1 = v1[2]
        Y1 = v1[4]
        K1 = v1[3]
        e1 = v1[5]
        pos1 = v1[6]
        pro11 = v1[7]
        pro12 = v1[8]
        et1 = v1[9]
        sp1 = v1[10]
    if k1 == vvod2:
        a2 = v1[0]
        dx2 = v1[1]
        kk2 = v1[2]
        Y2 = v1[4]
        K2 = v1[3]
        e2 = v1[5]
        pos2 = v1[6]
        pro21 = v1[7]
        pro22 = v1[8]
        et2 = v1[9]
        sp2 = v1[10]
    if k1 == vvod3:
        a3 = v1[0]
        dx3 = v1[1]
        kk3 = v1[2]
        Y3 = v1[4]
        K3 = v1[3]
        e3 = v1[5]
        pos3 = v1[6]
        pro31 = v1[7]
        pro32 = v1[8]
        et3 = v1[9]
        sp3 = v1[10]
    if k1 == vvod4:
        a4 = v1[0]
        dx4 = v1[1]
        kk4 = v1[2]
        Y4 = v1[4]
        K4 = v1[3]
        e4 = v1[5]
        pos4 = v1[6]
        pro41 = v1[7]
        pro42 = v1[8]
        et4 = v1[9]
        sp4 = v1[10]


dlina1 = kk1*kolvo1+kk2*kolvo2+kk3*kolvo3+kk4*kolvo4
dL = dlina-dlina1
Ld = dlina1/dlina
summa = kolvo1+kolvo2+kolvo3+kolvo4#сумма всех вводов
dxx = dL/(summa+1)#отсуп

#расстановка в 1ряд
if dL > 0 and (Ld < 1):
    koordinat1 = []

    if kolvo1 == 0:
        kk1 = kk2
    if kolvo2 == 0:
        kk2 = kk1
    if kolvo3 == 0:
        kk3 = kk2
    if kolvo4 == 0:
        kk4 = kk3
    xx1 = x2-dlina/2+dxx+kk1/2#расчет первой координаты    
    koordinat1.append(xx1)
    i = 0
    for k in range(kolvo1-1):
        i = i+1
        xx2 = koordinat1[-1+i]+kk1+dxx
        koordinat1.append(xx2)
    for k in range(kolvo2):
        i=i+1
        if k == 0:
            xx3 = koordinat1[-1+i]+kk1/2+kk2/2+dxx #при первом вводе
        else:
            xx3 = koordinat1[-1+i]+kk2+dxx
        koordinat1.append(xx3)
    for k in range(kolvo3):
        i=i+1
        if k==0:
            xx4=koordinat1[-1+i]+kk2/2+kk3/2+dxx #при первом вводе
        else:
            xx4=koordinat1[-1+i]+kk3+dxx
        koordinat1.append(xx4)
    for k in range(kolvo4):
        i = i+1
        if k == 0:
            xx5 = koordinat1[-1+i]+kk3/2+kk4/2+dxx #при первом вводе
        else:
            xx5 = koordinat1[-1+i]+kk4+dxx
        koordinat1.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat1)):
        if koordinat1[x] < x2:
            koordinat1[x] = koordinat1[x]-ror
        else:
            koordinat1[x] = koordinat1[x]+ror

    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et,temp):
        for k in range(kolvo):
            i = i+1
            #ввод
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0
            iPlacementParam.scale_ = 1
            iPlacementParam.xBase = koordinat1[-1+i]
            iPlacementParam.yBase = y2
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz == u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0
                iPlacementParam.scale_ = 1
                iPlacementParam.xBase = koordinat1[-1+i]
                iPlacementParam.yBase = y2
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if sp == u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(temp,temp,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0
                iPlacementParam.scale_ = 1
                iPlacementParam.xBase = koordinat1[-1+i]
                iPlacementParam.yBase = y2
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #проекции
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            if shir == 4:
                obj = iFragment.ksFragmentDefinition(pro1,pro1,0)
            if shir == 6:
                obj = iFragment.ksFragmentDefinition(pro2,pro2,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0
            iPlacementParam.scale_ = 1
            iPlacementParam.xBase = koordinat1[-1+i]
            iPlacementParam.yBase = y2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k == 0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0
                iPlacementParam.scale_ = 1
                iPlacementParam.xBase = koordinat1[-1+i]
                iPlacementParam.yBase = y2
                iFragment.ksReadFragment(pos, True, iPlacementParam)

    
    Vvod_input(i=0,kolvo=kolvo1,a=a1,pro1=pro11,pro2=pro12,pos=pos1,et=et1, temp=sp1)
    Vvod_input(i=kolvo1,kolvo=kolvo2,a=a2,pro1=pro21,pro2=pro22,pos=pos2,et=et2, temp=sp2)
    Vvod_input(i=kolvo1+kolvo2,kolvo=kolvo3,a=a3,pro1=pro31,pro2=pro32,pos=pos3,et=et3, temp=sp3)
    Vvod_input(i=kolvo1+kolvo2+kolvo3,kolvo=kolvo4,a=a4,pro1=pro41,pro2=pro42,pos=pos4,et=et4, temp=sp4)

#расстановка в 2ряда
if dL <= 0 and (1 <= Ld <= 1.8):
    #расчет координат
    if e1 == 2:
        kk1 = kk1*1.2
    if e2 == 2:
        kk2 = kk2*1.2
    if e3 == 2:
        kk3 = kk3*1.2
    if e4 == 2:
        kk4 = kk4*1.2
    
    dlina1 = kk1*kolvo1+kk2*kolvo2+kk3*kolvo3+kk4*kolvo4
    dlina = dlina*0.85
    
    dL = dlina-dlina1
    summa = kolvo1+kolvo2+kolvo3+kolvo4#сумма всех вводов
    dxx = dL/(summa+1)#отсуп
    koordinat1 = []

    if kolvo1 == 0:
        kk1 = kk2
    if kolvo2 == 0:
        kk2 = kk1
    if kolvo3 == 0:
        kk3 = kk2
    if kolvo4 == 0:
        kk4 = kk3
    xx1 = x2-dlina/2+dxx+kk1/2#расчет первой координаты    
    koordinat1.append(xx1)
    i = 0
    for k in range(kolvo1-1):
        i = i+1
        xx2 = koordinat1[-1+i]+kk1+dxx
        koordinat1.append(xx2)
    for k in range(kolvo2):
        i = i+1
        if k == 0:
            xx3 = koordinat1[-1+i]+kk1/2+kk2/2+dxx #при первом вводе
        else:
            xx3 = koordinat1[-1+i]+kk2+dxx
        koordinat1.append(xx3)
    for k in range(kolvo3):
        i = i+1
        if k == 0:
            xx4 = koordinat1[-1+i]+kk2/2+kk3/2+dxx #при первом вводе
        else:
            xx4 = koordinat1[-1+i]+kk3+dxx
        koordinat1.append(xx4)
    for k in range(kolvo4):
        i = i+1
        if k == 0:
            xx5 = koordinat1[-1+i]+kk3/2+kk4/2+dxx #при первом вводе
        else:
            xx5 = koordinat1[-1+i]+kk4+dxx
        koordinat1.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat1)):
        if koordinat1[x] < x2:
            koordinat1[x] = koordinat1[x]-ror
        else:
            koordinat1[x] = koordinat1[x]+ror

            
    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et,Y,temp):
        for k in range(kolvo):
            #ввод
            i = i+1
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0
            iPlacementParam.scale_ = 1
            iPlacementParam.xBase = koordinat1[-1+i]
            if i%2 == 0:
                iPlacementParam.yBase = y2+Y
            if i%2 == 1:
                iPlacementParam.yBase = y2-Y
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz == u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0
                iPlacementParam.scale_ = 1
                iPlacementParam.xBase = koordinat1[-1+i]
                if i%2==0:
                    iPlacementParam.yBase = y2+Y
                if i%2==1:
                    iPlacementParam.yBase = y2-Y
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if sp == u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(temp,temp,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0
                iPlacementParam.scale_ = 1
                iPlacementParam.xBase = koordinat1[-1+i]
                if i%2==0:
                    iPlacementParam.yBase = y2+Y
                if i%2==1:
                    iPlacementParam.yBase = y2-Y
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #проекции
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            if shir == 4:
                obj = iFragment.ksFragmentDefinition(pro1,pro1,0)
            if shir == 6:
                obj = iFragment.ksFragmentDefinition(pro2,pro2,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0
            iPlacementParam.scale_ = 1
            iPlacementParam.xBase = koordinat1[-1+i]
            iPlacementParam.yBase = y2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k == 0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0
                iPlacementParam.scale_ = 1
                iPlacementParam.xBase = koordinat1[-1+i]
                if i%2 == 0:
                    iPlacementParam.yBase = y2+Y
                if i%2 == 1:
                    iPlacementParam.yBase = y2-Y
                iFragment.ksReadFragment(pos, True, iPlacementParam)

    Vvod_input(i=0,kolvo=kolvo1,a=a1,pro1=pro11,pro2=pro12,pos=pos1,et=et1,Y=Y1,temp=sp1)
    Vvod_input(i=kolvo1,kolvo=kolvo2,a=a2,pro1=pro21,pro2=pro22,pos=pos2,et=et2,Y=Y2,temp=sp2)
    Vvod_input(i=kolvo1+kolvo2,kolvo=kolvo3,a=a3,pro1=pro31,pro2=pro32,pos=pos3,et=et3,Y=Y3,temp=sp3)
    Vvod_input(i=kolvo1+kolvo2+kolvo3,kolvo=kolvo4,a=a4,pro1=pro41,pro2=pro42,pos=pos4,et=et4,Y=Y4,temp=sp4)

#расстановка в 3ряда
if dL <= 0 and (1.8 < Ld <= 2.7):
    #расчет координат
    if e1 == 2:
        kk1 = kk1*1.2
    if e2 == 2:
        kk2 = kk2*1.2
    if e3 == 2:
        kk3 = kk3*1.2
    if e4 == 2:
        kk4 = kk4*1.2
    
    dlina1 = kk1*kolvo1+kk2*kolvo2+kk3*kolvo3+kk4*kolvo4
    
    if e1 == 1:
        dlina = dlina*0.86
    if e1 == 2:
        dlina = dlina*0.77
    
    
    dL = dlina-dlina1
    summa = kolvo1+kolvo2+kolvo3+kolvo4#сумма всех вводов
    dxx = dL/(summa+1)#отсуп
    koordinat1 = []

    if kolvo1 == 0:
        kk1 = kk2
    if kolvo2 == 0:
        kk2 = kk1
    if kolvo3 == 0:
        kk3 = kk2
    if kolvo4 == 0:
        kk4 = kk3
    xx1 = x2-dlina/2+dxx+kk1/2#расчет первой координаты    
    koordinat1.append(xx1)
    i = 0
    for k in range(kolvo1-1):
        i = i+1
        xx2 = koordinat1[-1+i]+kk1+dxx
        koordinat1.append(xx2)
    for k in range(kolvo2):
        i = i+1
        if k == 0:
            xx3 = koordinat1[-1+i]+kk1/2+kk2/2+dxx #при первом вводе
        else:
            xx3 = koordinat1[-1+i]+kk2+dxx
        koordinat1.append(xx3)
    for k in range(kolvo3):
        i = i+1
        if k == 0:
            xx4 = koordinat1[-1+i]+kk2/2+kk3/2+dxx #при первом вводе
        else:
            xx4 = koordinat1[-1+i]+kk3+dxx
        koordinat1.append(xx4)
    for k in range(kolvo4):
        i = i+1
        if k == 0:
            xx5 = koordinat1[-1+i]+kk3/2+kk4/2+dxx #при первом вводе
        else:
            xx5 = koordinat1[-1+i]+kk4+dxx
        koordinat1.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat1)):
        if koordinat1[x] < x2:
            koordinat1[x] = koordinat1[x]-ror
        else:
            koordinat1[x] = koordinat1[x]+ror

    

    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et,Y,temp):
        for k in range(kolvo):
            #ввод
            i = i+1
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0
            iPlacementParam.scale_ = 1
            iPlacementParam.xBase = koordinat1[-1+i]
                            
            if i%3 == 0:
                iPlacementParam.yBase = y2
            if i%3 == 1:
                iPlacementParam.yBase = y2-2*Y
            if i%3 == 2:
                iPlacementParam.yBase = y2+2*Y
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz == u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0
                iPlacementParam.scale_ = 1
                iPlacementParam.xBase = koordinat1[-1+i]
                if i%3 == 0:
                    iPlacementParam.yBase = y2
                if i%3 == 1:
                    iPlacementParam.yBase = y2-2*Y
                if i%3 == 2:
                    iPlacementParam.yBase = y2+2*Y
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if sp == u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(temp,temp,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0
                iPlacementParam.scale_ = 1
                iPlacementParam.xBase = koordinat1[-1+i]
                if i%3 == 0:
                    iPlacementParam.yBase = y2
                if i%3 == 1:
                    iPlacementParam.yBase = y2-2*Y
                if i%3 == 2:
                    iPlacementParam.yBase = y2+2*Y
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #проекции
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            if shir == 4:
                obj = iFragment.ksFragmentDefinition(pro1,pro1,0)
            if shir == 6:
                obj = iFragment.ksFragmentDefinition(pro2,pro2,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0
            iPlacementParam.scale_ = 1
            iPlacementParam.xBase = koordinat1[-1+i]
            iPlacementParam.yBase = y2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k == 0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0
                iPlacementParam.scale_ = 1
                iPlacementParam.xBase = koordinat1[-1+i]
                if i%3 == 0:
                    iPlacementParam.yBase = y2
                if i%3 == 1:
                    iPlacementParam.yBase = y2-2*Y
                if i%3 == 2:
                    iPlacementParam.yBase = y2+2*Y
                iFragment.ksReadFragment(pos, True, iPlacementParam)

    Vvod_input(i=0,kolvo=kolvo1,a=a1,pro1=pro11,pro2=pro12,pos=pos1,et=et1,Y=Y1,temp=sp1)
    Vvod_input(i=kolvo1,kolvo=kolvo2,a=a2,pro1=pro21,pro2=pro22,pos=pos2,et=et2,Y=Y2,temp=sp2)
    Vvod_input(i=kolvo1+kolvo2,kolvo=kolvo3,a=a3,pro1=pro31,pro2=pro32,pos=pos3,et=et3,Y=Y3,temp=sp3)
    Vvod_input(i=kolvo1+kolvo2+kolvo3,kolvo=kolvo4,a=a4,pro1=pro41,pro2=pro42,pos=pos4,et=et4,Y=Y4,temp=sp4)


#расстановка в 4ряда
if dL <= 0 and (2.7 < Ld <= 3.7):
    #расчет координат
    if e1 == 2:
        kk1 = kk1*1.2
    if e2 == 2:
        kk2 = kk2*1.2
    if e3 == 2:
        kk3 = kk3*1.2
    if e4 == 2:
        kk4 = kk4*1.2
    
    dlina1 = kk1*kolvo1+kk2*kolvo2+kk3*kolvo3+kk4*kolvo4
    if e1 == 1:
