            iPlacementParam.yBase = y2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k==0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0
                iPlacementParam.scale_ = 1
                iPlacementParam.xBase = koordinat33[-1+i]
                iPlacementParam.yBase = y2-kk
                iFragment.ksReadFragment(pos, True, iPlacementParam)

    if (kolvo9+kolvo10)%3 == 0:
        Vvod_input(i=0,kolvo=kolvo99-kolvo10,a=a9,pro1=pro91,pro2=pro92,pos=pos9,et=et9,temp=sp9,kk=0)
        if kk9 <= kk10:
            Vvod_input(i=0,kolvo=kolvo99,a=a9,pro1=pro91,pro2=pro92,pos=pos9,et=et9,temp=sp9,kk=-kk10)
            Vvod_input(i=0,kolvo=kolvo99,a=a9,pro1=pro91,pro2=pro92,pos=pos9,et=et9,temp=sp9,kk=kk10)
        if kk9 > kk10:
            Vvod_input(i=0,kolvo=kolvo99,a=a9,pro1=pro91,pro2=pro92,pos=pos9,et=et9,temp=sp9,kk=-kk9)
            Vvod_input(i=0,kolvo=kolvo99,a=a9,pro1=pro91,pro2=pro92,pos=pos9,et=et9,temp=sp9,kk=kk9)

        Vvod_input(i=kolvo99-kolvo10,kolvo=kolvo10,a=a10,pro1=pro101,pro2=pro102,pos=pos10,et=et10,temp=sp10,kk=0)
        
        
    
    if (kolvo9+kolvo10)%3 == 1:
        Vvod_input(i=0,kolvo=kolvo99-kolvo10,a=a9,pro1=pro91,pro2=pro92,pos=pos9,et=et9,temp=sp9,kk=0)
        if kk9 <= kk10:
            Vvod_input(i=0,kolvo=kolvo99,a=a9,pro1=pro91,pro2=pro92,pos=pos9,et=et9,temp=sp9,kk=-kk10)
            Vvod_input2(i=0,kolvo=kolvo999,a=a9,pro1=pro91,pro2=pro92,pos=pos9,et=et9,temp=sp9,kk=kk10)
