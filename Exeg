                if i%2==0:
                    iPlacementParam.yBase = y2+Y
                if i%2==1:
                    iPlacementParam.yBase = y2-Y
                iFragment.ksReadFragment(pos, True, iPlacementParam)
                

    Vvod_input(i=0,kolvo=kolvo9,a=a9,pro1=pro91,pro2=pro92,pos=pos9,et=et9,Y=Y9,temp=sp9)
    Vvod_input(i=kolvo9,kolvo=kolvo10,a=a10,pro1=pro101,pro2=pro102,pos=pos10,et=et10,Y=Y10,temp=sp10)
    Vvod_input(i=kolvo9+kolvo10,kolvo=kolvo11,a=a11,pro1=pro111,pro2=pro112,pos=pos11,et=et11,Y=Y11,temp=sp11)
    Vvod_input(i=kolvo9+kolvo10+kolvo11,kolvo=kolvo12,a=a12,pro1=pro121,pro2=pro122,pos=pos12,et=et12,Y=Y12,temp=sp12)

#расстановка в 3ряда
if dL <= 0 and (1.8 < Ld <= 3):

    
    if kolvo9==0:
        kk9=kk10      
    if kolvo10==0:
        kk10=kk9
    if kolvo11==0:
        kk11=kk10
    if kolvo12==0:
        kk12=kk11

    #в случае если на 3 делится
    if (kolvo9+kolvo10)%3 == 0:
        kolvo99 = (kolvo9+kolvo10)/3

    
        koordinat3=[]
        #dlina1 = kk9*kolvo99+kk10*kolvo10+kk11*kolvo11+kk12*kolvo12+kk13*kolvo13+kk14*kolvo14+kk15*kolvo15+kk16*kolvo16
        dlina1 = kk9*kolvo99
        dL = dlina-dlina1
        Ld = dlina1/dlina
        #summa = kolvo99+kolvo10+kolvo11+kolvo12+kolvo13+kolvo14+kolvo15+kolvo16#сумма всех вводов
        summa = kolvo99
        dxx = dL/(summa+1)#отсуп
            
        
                    
        xx1=x2-dlina/2+dxx+kk9/2#расчет первой координаты    
        koordinat3.append(xx1)
        i=0
        for k in range(kolvo99-1):
            i=i+1
            xx2=koordinat3[-1+i]+kk9+dxx
            koordinat3.append(xx2)
        for k in range(kolvo10):
            i=i+1
            if k==0:
                xx3=koordinat3[-1+i]+kk9/2+kk10/2+dxx #при первом вводе
            else:
                xx3=koordinat3[-1+i]+kk10+dxx
            koordinat3.append(xx3)
        for k in range(kolvo11):
            i=i+1
            if k==0:
                xx4=koordinat3[-1+i]+kk10/2+kk11/2+dxx #при первом вводе
            else:
                xx4=koordinat3[-1+i]+kk11+dxx
            koordinat3.append(xx4)
        for k in range(kolvo12):
            i=i+1
            if k==0:
                xx5=koordinat3[-1+i]+kk11/2+kk12/2+dxx #при первом вводе
            else:
                xx5=koordinat3[-1+i]+kk12+dxx
            koordinat3.append(xx5)
        #напишем блок для смещения для длинных корпусов
        for x in range(len(koordinat3)):
            if koordinat3[x]<x2:
                koordinat3[x]=koordinat3[x]-ror
            else:
                koordinat3[x]=koordinat3[x]+ror

    #в случае если на 3 делится +1
    if (kolvo9+kolvo10)%3 == 1:
        kolvo99 = (kolvo9+kolvo10)/3

    
        koordinat3=[]
        dlina1 = kk9*kolvo99
        dL = dlina-dlina1
        Ld = dlina1/dlina
        summa = kolvo99#сумма всех вводов
        dxx = dL/(summa+1)#отсуп
        
                    
        xx1=x2-dlina/2+dxx+kk9/2#расчет первой координаты    
        koordinat3.append(xx1)
        i=0
        for k in range(kolvo99-1):
            i=i+1
            xx2=koordinat3[-1+i]+kk9+dxx
            koordinat3.append(xx2)
        for k in range(kolvo10):
            i=i+1
            if k==0:
                xx3=koordinat3[-1+i]+kk9/2+kk10/2+dxx #при первом вводе
            else:
                xx3=koordinat3[-1+i]+kk10+dxx
            koordinat3.append(xx3)
        for k in range(kolvo11):
            i=i+1
            if k==0:
                xx4=koordinat3[-1+i]+kk10/2+kk11/2+dxx #при первом вводе
            else:
                xx4=koordinat3[-1+i]+kk11+dxx
            koordinat3.append(xx4)
        for k in range(kolvo12):
            i=i+1
            if k==0:
                xx5=koordinat3[-1+i]+kk11/2+kk12/2+dxx #при первом вводе
            else:
                xx5=koordinat3[-1+i]+kk12+dxx
            koordinat3.append(xx5)
        #напишем блок для смещения для длинных корпусов
        for x in range(len(koordinat3)):
            if koordinat3[x]<x2:
                koordinat3[x]=koordinat3[x]-ror
            else:
                koordinat3[x]=koordinat3[x]+ror

        
        #запишем координаты для +1
        kolvo999 = (kolvo9+kolvo10)/3+1
        koordinat33=[]
        dlina1 = kk9*kolvo999
        dL = dlina-dlina1
        Ld = dlina1/dlina
        summa = kolvo999#сумма всех вводов
        dxx = dL/(summa+1)#отсуп
                
                    
        xx1=x2-dlina/2+dxx+kk9/2#расчет первой координаты    
        koordinat33.append(xx1)
        i=0
        for k in range(kolvo999-1):
            i=i+1
            xx2=koordinat33[-1+i]+kk9+dxx
            koordinat33.append(xx2)
        for k in range(kolvo10):
            i=i+1
            if k==0:
                xx3=koordinat33[-1+i]+kk9/2+kk10/2+dxx #при первом вводе
            else:
                xx3=koordinat33[-1+i]+kk10+dxx
            koordinat33.append(xx3)
        for k in range(kolvo11):
            i=i+1
            if k==0:
                xx4=koordinat33[-1+i]+kk10/2+kk11/2+dxx #при первом вводе
            else:
                xx4=koordinat33[-1+i]+kk11+dxx
            koordinat33.append(xx4)
        for k in range(kolvo12):
            i=i+1
            if k==0:
                xx5=koordinat33[-1+i]+kk11/2+kk12/2+dxx #при первом вводе
            else:
                xx5=koordinat33[-1+i]+kk12+dxx
            koordinat33.append(xx5)
        #напишем блок для смещения для длинных корпусов
        for x in range(len(koordinat3)):
            if koordinat33[x]<x2:
                koordinat33[x]=koordinat33[x]-ror
            else:
                koordinat33[x]=koordinat33[x]+ror

    #в случае если на 3 делится +2
    if (kolvo9+kolvo10)%3 == 2:
        kolvo99 = (kolvo9+kolvo10)/3

    
