if drenazh == u'да ТГК':
    if kolvo12 == 0:
        if Mat == 0:
            a12 = box_vvod[756]
            pos12 = box_vvod[760]
        if Mat == 1:
            a12 = box_vvod[757]
            pos12 = box_vvod[761]
        if material == 2:
            a12 = box_vvod[816]
            pos12 = box_vvod[817]
            
