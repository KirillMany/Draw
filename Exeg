                  u'BM S110':[286,4,245,315,520, 15,16,25,38.2,60,74,70, 22,20.5,22.65,19.75,12.75,7,0],
                  
                      u'BM S111':[286,4,225,315,540, 19,30,45,50,60,70,70, 12.7,10.41,5.73,0,0,0,0],
                  u'BM S112':[324,4,302,472.4,658, 19,30,45,50,60,70,70, 12.7,10.41,5.73,0,0,0,0],
                  
                      u'BM S114':[376,4,370,570,600, 15,16,25,38.2,60,74,70, 22,20.5,22.65,19.75,12.75,7,0],
                  
                  u'BM S115':[368,4,370,520,600, 19,30,45,50,60,70,70, 12.7,10.41,5.73,0,0,0,0],
                      u'BM S117':[482,4,320,420,700, 19,30,45,50,60,70,70, 12.7,10.41,5.73,0,0,0,0],
                     
        }


for k, v in dict_korpus_data2.items(): 
    if k == x1:
        dlina = v[0]
        shir = v[1]
        x2 = v[2]
        y2 = v[3]
        y22 = v[4]

        kk00 = v[5]
        kk22 = v[6]
        kk33 = v[7]
        kk44 = v[8]
        kk55 = v[9]
        kk66 = v[10]
        kk77 = v[11]

        yy0 = v[12]
        yy2 = v[13]
        yy3 = v[14]
        yy4 = v[15]
        yy5 = v[16]
        yy6 = v[17]
        yy7 = v[18]


#словарь для определения вводов стороны B
a5=0
a6=0
a7=0
a8=0

dx5=0
dx6=0
dx7=0
dx8=0

kk5=0
kk6=0
kk7=0
kk8=0

Y5=0
Y6=0
Y7=0
Y8=0

K5=0
K6=0
K7=0
K8=0

e5=0
e6=0
e7=0
e8=0

pos5=0
pos6=0
pos7=0
pos8=0

pro51=0
pro61=0
pro71=0
pro81=0

pro52=0
pro62=0
pro72=0
pro82=0

et5=0
et6=0
et7=0
et8=0

sp5 = 0
sp6 = 0
sp7 = 0
sp8 = 0

ror = 0
if x1 == u'TM A139' or x1 == u'BM A139':
    ror = 28

for k5, v5, in dict_vvod_data1.items():
    if k5==vvod5:
        a5=v5[0]
        dx5=v5[1]
        kk5=v5[2]
        Y5=v5[4]
        K5=v5[3]
        e5=v5[5]
        pos5=v5[6]
        pro51=v5[7]
        pro52=v5[8]
        et5=v5[9]
        sp5=v5[10]
    if k5==vvod6:
        a6=v5[0]
        dx6=v5[1]
        kk6=v5[2]
        Y6=v5[4]
        K6=v5[3]
        e6=v5[5]
        pos6=v5[6]
        pro61=v5[7]
        pro62=v5[8]
        et6=v5[9]
        sp6=v5[10]
    if k5==vvod7:
        a7=v5[0]
        dx7=v5[1]
        kk7=v5[2]
        Y7=v5[4]
        K7=v5[3]
        e7=v5[5]
        pos7=v5[6]
        pro71=v5[7]
        pro72=v5[8]
        et7=v5[9]
        sp7=v5[10]
    if k5==vvod8:
        a8=v5[0]
        dx8=v5[1]
        kk8=v5[2]
        Y8=v5[4]
        K8=v5[3]
        e8=v5[5]
        pos8=v5[6]
        pro81=v5[7]
        pro82=v5[8]
        et8=v5[9]
        sp8=v5[10]
    
    
dlina1 = kk5*kolvo5+kk6*kolvo6+kk7*kolvo7+kk8*kolvo8
dL = dlina-dlina1
Ld = dlina1/dlina
summa = kolvo5+kolvo6+kolvo7+kolvo8#сумма всех вводов
dxx = dL/(summa+1)#отсуп

#расстановка в 1ряд
if dL > 0 and (Ld < 1):
    koordinat2=[]

    if kolvo5==0:
        kk5=kk6
    if kolvo6==0:
        kk6=kk5
    if kolvo7==0:
        kk7=kk6
    if kolvo8==0:
        kk8=kk7
    xx1=y2-dlina/2+dxx+kk5/2#расчет первой координаты    
    koordinat2.append(xx1)
    i=0
    for k in range(kolvo5-1):
        i=i+1
        xx2=koordinat2[-1+i]+kk5+dxx
        koordinat2.append(xx2)
    for k in range(kolvo6):
        i=i+1
        if k==0:
            xx3=koordinat2[-1+i]+kk5/2+kk6/2+dxx #при первом вводе
        else:
            xx3=koordinat2[-1+i]+kk6+dxx
        koordinat2.append(xx3)
    for k in range(kolvo7):
        i=i+1
        if k==0:
            xx4=koordinat2[-1+i]+kk6/2+kk7/2+dxx #при первом вводе
        else:
            xx4=koordinat2[-1+i]+kk7+dxx
        koordinat2.append(xx4)
    for k in range(kolvo8):
        i=i+1
        if k==0:
            xx5=koordinat2[-1+i]+kk7/2+kk8/2+dxx #при первом вводе
        else:
            xx5=koordinat2[-1+i]+kk8+dxx
        koordinat2.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat2)):
        if koordinat2[x]<x2:
            koordinat2[x]=koordinat2[x]-ror
        else:
            koordinat2[x]=koordinat2[x]+ror

    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et,temp):
        for k in range(kolvo):
            i=i+1
            #ввод
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0
            iPlacementParam.scale_ = 1
            iPlacementParam.yBase = koordinat2[-1+i]
            iPlacementParam.xBase = x2
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = -45
                iPlacementParam.scale_ = 1
                iPlacementParam.yBase = koordinat2[-1+i]
                iPlacementParam.xBase = x2
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if sp==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(temp,temp,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = -45
                iPlacementParam.scale_ = 1
                iPlacementParam.yBase = koordinat2[-1+i]
                iPlacementParam.xBase = x2
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #проекции
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            if shir==4:
                obj = iFragment.ksFragmentDefinition(pro1,pro1,0)
            if shir==6:
                obj = iFragment.ksFragmentDefinition(pro2,pro2,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = -90
            iPlacementParam.scale_ = 1
            iPlacementParam.yBase = koordinat2[-1+i]
            iPlacementParam.xBase = x2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k==0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0
                iPlacementParam.scale_ = 1
                iPlacementParam.yBase = koordinat2[-1+i]
                iPlacementParam.xBase = x2
                iFragment.ksReadFragment(pos, True, iPlacementParam)

    Vvod_input(i=0,kolvo=kolvo5,a=a5,pro1=pro51,pro2=pro52,pos=pos5,et=et5,temp=sp5)
    Vvod_input(i=kolvo5,kolvo=kolvo6,a=a6,pro1=pro61,pro2=pro62,pos=pos6,et=et6,temp=sp6)
    Vvod_input(i=kolvo5+kolvo6,kolvo=kolvo7,a=a7,pro1=pro71,pro2=pro72,pos=pos7,et=et7,temp=sp7)
    Vvod_input(i=kolvo5+kolvo6+kolvo7,kolvo=kolvo8,a=a8,pro1=pro81,pro2=pro82,pos=pos8,et=et8,temp=sp8)
    
#расстановка в 2ряда
if dL <= 0 and (1 <= Ld <= 1.8):
    #расчет координат
    if e5==2:
        kk5=kk5*1.2
    if e6==2:
        kk6=kk6*1.2
    if e7==2:
        kk7=kk7*1.2
    if e8==2:
        kk8=kk8*1.2
    
    dlina1=kk5*kolvo5+kk6*kolvo6+kk7*kolvo7+kk8*kolvo8
    dlina=dlina*0.8
    
    dL=dlina-dlina1
    summa=kolvo5+kolvo6+kolvo7+kolvo8#сумма всех вводов
    dxx=dL/(summa+1)#отсуп
    koordinat2=[]

    if kolvo5==0:
        kk5=kk6
    if kolvo6==0:
        kk6=kk5
    if kolvo7==0:
        kk7=kk6
    if kolvo8==0:
        kk8=kk7
    xx1=y2-dlina/2+dxx+kk5/2#расчет первой координаты    
    koordinat2.append(xx1)
    i=0
    for k in range(kolvo5-1):
        i=i+1
        xx2=koordinat2[-1+i]+kk5+dxx
        koordinat2.append(xx2)
    for k in range(kolvo6):
        i=i+1
        if k==0:
            xx3=koordinat2[-1+i]+kk5/2+kk6/2+dxx #при первом вводе
        else:
            xx3=koordinat2[-1+i]+kk6+dxx
        koordinat2.append(xx3)
    for k in range(kolvo7):
        i=i+1
        if k==0:
            xx4=koordinat2[-1+i]+kk6/2+kk7/2+dxx #при первом вводе
        else:
            xx4=koordinat2[-1+i]+kk7+dxx
        koordinat2.append(xx4)
    for k in range(kolvo8):
        i=i+1
        if k==0:
            xx5=koordinat2[-1+i]+kk7/2+kk8/2+dxx #при первом вводе
        else:
            xx5=koordinat2[-1+i]+kk8+dxx
        koordinat2.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat2)):
        if koordinat2[x]<x2:
            koordinat2[x]=koordinat2[x]-ror
        else:
            koordinat2[x]=koordinat2[x]+ror

            
    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et,Y,temp):
        for k in range(kolvo):
            #ввод
            i=i+1
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0
            iPlacementParam.scale_ = 1
            iPlacementParam.yBase = koordinat2[-1+i]
            if i%2==0:
                iPlacementParam.xBase = x2+Y
            if i%2==1:
                iPlacementParam.xBase = x2-Y
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = -45
                iPlacementParam.scale_ = 1
                iPlacementParam.yBase = koordinat2[-1+i]
                if i%2==0:
                    iPlacementParam.xBase = x2+Y
                if i%2==1:
                    iPlacementParam.xBase = x2-Y
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if sp==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(temp,temp,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = -45
                iPlacementParam.scale_ = 1
                iPlacementParam.yBase = koordinat2[-1+i]
                if i%2==0:
                    iPlacementParam.xBase = x2+Y
                if i%2==1:
                    iPlacementParam.xBase = x2-Y
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #проекции
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            if shir==4:
                obj = iFragment.ksFragmentDefinition(pro1,pro1,0)
            if shir==6:
                obj = iFragment.ksFragmentDefinition(pro2,pro2,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = -90
            iPlacementParam.scale_ = 1
            iPlacementParam.yBase = koordinat2[-1+i]
            iPlacementParam.xBase = x2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k==0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0
                iPlacementParam.scale_ = 1
                iPlacementParam.yBase = koordinat2[-1+i]
                if i%2==0:
                    iPlacementParam.xBase = x2+Y
                if i%2==1:
                    iPlacementParam.xBase = x2-Y
                iFragment.ksReadFragment(pos, True, iPlacementParam)

    Vvod_input(i=0,kolvo=kolvo5,a=a5,pro1=pro51,pro2=pro52,pos=pos5,et=et5,Y=Y5,temp=sp5)
    Vvod_input(i=kolvo5,kolvo=kolvo6,a=a6,pro1=pro61,pro2=pro62,pos=pos6,et=et6,Y=Y6,temp=sp6)
    Vvod_input(i=kolvo5+kolvo6,kolvo=kolvo7,a=a7,pro1=pro71,pro2=pro72,pos=pos7,et=et7,Y=Y7,temp=sp7)
