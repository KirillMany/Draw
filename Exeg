            obj = iFragment.ksFragmentDefinition(box_Peremichka[7], box_Peremichka[7], 0)
        if sechenie1==4 and peremichka1==u'4-х полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[8], box_Peremichka[8], 0)
        if sechenie1==4 and peremichka1==u'5-и полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[9], box_Peremichka[9], 0)
        if sechenie1==4 and peremichka1==u'10-и полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[10], box_Peremichka[10], 0)
        if sechenie1==4 and peremichka1==u'20-и полюс':
            obj = iFragment.ksFragmentDefinition(box_Peremichka[11], box_Peremichka[11], 0)

