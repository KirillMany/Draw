c00 = 0
c11 = 0
c22 = 0
c33 = 0
c44 = 0
c55 = 0
#переменная для пружинных концевых
if (c0+c0bu+c0pe) > 0:
    c00 = 2.2
if (c1+c1bu+c1pe) != 0:
    c11 = 2.2
if (c2+c2bu+c2pe) > 0:
    c22 = 2.2
if (c3+c3bu+c3pe) > 0:
    c33 = 2.2
if (c4+c4bu+c4pe) > 0:
    c44 = 2.3
if (c5+c5bu+c5pe) > 0:
    c55 = 2.2
