    koordinat2.append(xx1)
    i=0
    for k in range(kolvo5-1):
        i=i+1
        xx2=koordinat2[-1+i]+kk5+dxx
        koordinat2.append(xx2)
    for k in range(kolvo6):
        i=i+1
        if k==0:
            xx3=koordinat2[-1+i]+kk5/2+kk6/2+dxx #при первом вводе
        else:
            xx3=koordinat2[-1+i]+kk6+dxx
        koordinat2.append(xx3)
    for k in range(kolvo7):
        i=i+1
        if k==0:
            xx4=koordinat2[-1+i]+kk6/2+kk7/2+dxx #при первом вводе
        else:
            xx4=koordinat2[-1+i]+kk7+dxx
        koordinat2.append(xx4)
