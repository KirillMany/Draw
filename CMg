#функция для определения координат вставки объекта
def placement():
    iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
    iPlacementParam.Init()
    iPlacementParam.angle = 0.0
    iPlacementParam.scale_ = 1.0
    iPlacementParam.xBase = x2
    iPlacementParam.yBase = y2
    iFragment.ksInsertFragmentEx(obj, True, iPlacementParam, 1)
    
