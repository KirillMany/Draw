def incomeview11():
    iViewParam = KAPI.ksViewParam(iKompasObject.GetParamStruct(LDefin2D.ko_ViewParam))
    iViewParam.Init()
    iViewParam.angle = 0.0
    iViewParam.color = 0
    iViewParam.name = "Вид 10"
    iViewParam.scale_ = 0.1
    iViewParam.state = 3
    iViewParam.x = 0.0
    iViewParam.y = 0.0
    obj = iDocument2D.ksCreateSheetView(iViewParam, 0)
    #функция для вставки вида 1:1


def incomeview111(a):
    iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
    obj = iFragment.ksFragmentDefinition(a, a, 0)
    iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
    iPlacementParam.Init()
    iPlacementParam.angle = 0
    iPlacementParam.scale_ = 1.0
    iPlacementParam.yBase = 2400
    iPlacementParam.xBase = 670
    iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)


for k, v in dict_zip.items():
    if k==zp:
        incomeview11()
        for k in range(zpkolvo1):
            incomeview111(v)
    if k==zp1:
        incomeview11()
        for k in range(zpkolvo2):
            incomeview111(v)
    if k==zp2:
        incomeview11()
        for k in range(zpkolvo3):
            incomeview111(v)


