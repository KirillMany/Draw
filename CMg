pro22=0
pro32=0
pro42=0

et1=0
et2=0
et3=0
et4=0

ror=0
if x1 == u'CM P116' or x1 == u'CM A121' or x1 == u'CM A132'or x1 == u'CM A137'or x1 == u'CM A138'or x1 == u'CM A139':
    ror=28

for k1, v1, in dict_vvod_data1.items():
    if k1==vvod1:
        a1=v1[0]
        dx1=v1[1]
        kk1=v1[2]

        Y1=v1[4]
        K1=v1[3]
        e1=v1[5]

        pos1=v1[6]
        pro11=v1[7]
        pro12=v1[8]

        et1=v1[9]
        
    
for k2, v2, in dict_vvod_data1.items(): 
    if k2==vvod2:
        a2=v2[0]
        dx2=v2[1]
        kk2=v2[2]
        
        
        Y2=v2[4]
        K2=v2[3]
        e2=v2[5]

        pos2=v2[6]
        pro21=v2[7]
        pro22=v2[8]

        et2=v2[9]
        
for k3, v3, in dict_vvod_data1.items(): 
    if k3==vvod3:
        a3=v3[0]
        dx3=v3[1]
        kk3=v3[2]

        Y3=v3[4]
        K3=v3[3]
        e3=v3[5]

        pos3=v3[6]
        pro31=v3[7]
        pro32=v3[8]

        et3=v3[9]

for k4, v4, in dict_vvod_data1.items(): 
    if k4==vvod4:
        a4=v4[0]
        dx4=v4[1]
        kk4=v4[2]

        Y4=v4[4]
        K4=v4[3]
        e4=v4[5]

        pos4=v4[6]
        pro41=v4[7]
        pro42=v4[8]

        et4=v4[9]

dlina1=kk1*kolvo1+kk2*kolvo2+kk3*kolvo3+kk4*kolvo4
dL=dlina-dlina1
summa=kolvo1+kolvo2+kolvo3+kolvo4#сумма всех вводов
dxx=dL/(summa+1)#отсуп

if dL>0:
    koordinat1=[]

    if kolvo1==0:
        kk1=kk2
    if kolvo2==0:
        kk2=kk1
    if kolvo3==0:
        kk3=kk2
    if kolvo4==0:
        kk4=kk3
    xx1=x2-dlina/2+dxx+kk1/2#расчет первой координаты    
    koordinat1.append(xx1)
    i=0
    for k in range(kolvo1-1):
        i=i+1
        xx2=koordinat1[-1+i]+kk1+dxx
        koordinat1.append(xx2)
    for k in range(kolvo2):
        i=i+1
        if k==0:
            xx3=koordinat1[-1+i]+kk1/2+kk2/2+dxx #при первом вводе
        else:
            xx3=koordinat1[-1+i]+kk2+dxx
        koordinat1.append(xx3)
    for k in range(kolvo3):
        i=i+1
        if k==0:
            xx4=koordinat1[-1+i]+kk2/2+kk3/2+dxx #при первом вводе
        else:
            xx4=koordinat1[-1+i]+kk3+dxx
        koordinat1.append(xx4)
    for k in range(kolvo4):
        i=i+1
        if k==0:
            xx5=koordinat1[-1+i]+kk3/2+kk4/2+dxx #при первом вводе
        else:
            xx5=koordinat1[-1+i]+kk4+dxx
        koordinat1.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat1)):
        if koordinat1[x]<x2:
            koordinat1[x]=koordinat1[x]-ror
        else:
            koordinat1[x]=koordinat1[x]+ror

    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et):
        for k in range(kolvo):
            i=i+1
            #ввод
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0.0
            iPlacementParam.scale_ = 1.0
            iPlacementParam.xBase = koordinat1[-1+i]
            iPlacementParam.yBase = y2
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.xBase = koordinat1[-1+i]
                iPlacementParam.yBase = y2
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #проекции
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            if shir==4:
                obj = iFragment.ksFragmentDefinition(pro1,pro1,0)
            if shir==6:
                obj = iFragment.ksFragmentDefinition(pro2,pro2,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0.0
            iPlacementParam.scale_ = 1.0
            iPlacementParam.xBase = koordinat1[-1+i]
            iPlacementParam.yBase = y2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k==0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.xBase = koordinat1[-1+i]
                iPlacementParam.yBase = y2
                iFragment.ksReadFragment(pos, True, iPlacementParam)

    Vvod_input(i=0,kolvo=kolvo1,a=a1,pro1=pro11,pro2=pro12,pos=pos1,et=et1)
    Vvod_input(i=kolvo1,kolvo=kolvo2,a=a2,pro1=pro21,pro2=pro22,pos=pos2,et=et2)
    Vvod_input(i=kolvo1+kolvo2,kolvo=kolvo3,a=a3,pro1=pro31,pro2=pro32,pos=pos3,et=et3)
    Vvod_input(i=kolvo1+kolvo2+kolvo3,kolvo=kolvo4,a=a4,pro1=pro41,pro2=pro42,pos=pos4,et=et4)

if dL<=0:
    #расчет координат
    if e1==2:
        kk1=kk1*1.2
    if e2==2:
        kk2=kk2*1.2
    if e3==2:
        kk3=kk3*1.2
    if e4==2:
        kk4=kk4*1.2
    
    dlina1=kk1*kolvo1+kk2*kolvo2+kk3*kolvo3+kk4*kolvo4
    dlina=dlina*0.9
    
    dL=dlina-dlina1
    summa=kolvo1+kolvo2+kolvo3+kolvo4#сумма всех вводов
    dxx=dL/(summa+1)#отсуп
    koordinat1=[]

    if kolvo1==0:
        kk1=kk2
    if kolvo2==0:
        kk2=kk1
    if kolvo3==0:
        kk3=kk2
    if kolvo4==0:
        kk4=kk3
    xx1=x2-dlina/2+dxx+kk1/2#расчет первой координаты    
    koordinat1.append(xx1)
    i=0
    for k in range(kolvo1-1):
        i=i+1
        xx2=koordinat1[-1+i]+kk1+dxx
        koordinat1.append(xx2)
    for k in range(kolvo2):
        i=i+1
        if k==0:
            xx3=koordinat1[-1+i]+kk1/2+kk2/2+dxx #при первом вводе
        else:
            xx3=koordinat1[-1+i]+kk2+dxx
        koordinat1.append(xx3)
    for k in range(kolvo3):
        i=i+1
        if k==0:
            xx4=koordinat1[-1+i]+kk2/2+kk3/2+dxx #при первом вводе
        else:
            xx4=koordinat1[-1+i]+kk3+dxx
        koordinat1.append(xx4)
    for k in range(kolvo4):
        i=i+1
        if k==0:
            xx5=koordinat1[-1+i]+kk3/2+kk4/2+dxx #при первом вводе
        else:
            xx5=koordinat1[-1+i]+kk4+dxx
        koordinat1.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat1)):
        if koordinat1[x]<x2:
            koordinat1[x]=koordinat1[x]-ror
        else:
            koordinat1[x]=koordinat1[x]+ror
    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et,Y):
        for k in range(kolvo):
            #ввод
            i=i+1
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0.0
            iPlacementParam.scale_ = 1.0
            iPlacementParam.xBase = koordinat1[-1+i]
            if i%2==0:
                iPlacementParam.yBase = y2+Y
            if i%2==1:
                iPlacementParam.yBase = y2-Y
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.xBase = koordinat1[-1+i]
                if i%2==0:
                    iPlacementParam.yBase = y2+Y
                if i%2==1:
                    iPlacementParam.yBase = y2-Y
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #проекции
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            if shir==4:
                obj = iFragment.ksFragmentDefinition(pro1,pro1,0)
            if shir==6:
                obj = iFragment.ksFragmentDefinition(pro2,pro2,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0.0
            iPlacementParam.scale_ = 1.0
            iPlacementParam.xBase = koordinat1[-1+i]
            iPlacementParam.yBase = y2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k==0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.xBase = koordinat1[-1+i]
                if i%2==0:
                    iPlacementParam.yBase = y2+Y
                if i%2==1:
                    iPlacementParam.yBase = y2-Y
                iFragment.ksReadFragment(pos, True, iPlacementParam)

    Vvod_input(i=0,kolvo=kolvo1,a=a1,pro1=pro11,pro2=pro12,pos=pos1,et=et1,Y=Y1)
    Vvod_input(i=kolvo1,kolvo=kolvo2,a=a2,pro1=pro21,pro2=pro22,pos=pos2,et=et2,Y=Y2)
    Vvod_input(i=kolvo1+kolvo2,kolvo=kolvo3,a=a3,pro1=pro31,pro2=pro32,pos=pos3,et=et3,Y=Y3)
    Vvod_input(i=kolvo1+kolvo2+kolvo3,kolvo=kolvo4,a=a4,pro1=pro41,pro2=pro42,pos=pos4,et=et4,Y=Y4)

