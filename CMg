dlina1=kk5*kolvo5+kk6*kolvo6+kk7*kolvo7+kk8*kolvo8
dL=dlina-dlina1
summa=kolvo5+kolvo6+kolvo7+kolvo8#сумма всех вводов
dxx=dL/(summa+1)#отсуп

if dL>0:
    koordinat2=[]

    if kolvo5==0:
        kk5=kk6
    if kolvo6==0:
        kk6=kk5
    if kolvo7==0:
        kk7=kk6
    if kolvo8==0:
        kk8=kk7
    xx1=y2-dlina/2+dxx+kk5/2#расчет первой координаты    
    koordinat2.append(xx1)
    i=0
    for k in range(kolvo5-1):
        i=i+1
        xx2=koordinat2[-1+i]+kk5+dxx
        koordinat2.append(xx2)
    for k in range(kolvo6):
        i=i+1
        if k==0:
            xx3=koordinat2[-1+i]+kk5/2+kk6/2+dxx #при первом вводе
        else:
            xx3=koordinat2[-1+i]+kk6+dxx
        koordinat2.append(xx3)
    for k in range(kolvo7):
        i=i+1
        if k==0:
            xx4=koordinat2[-1+i]+kk6/2+kk7/2+dxx #при первом вводе
        else:
            xx4=koordinat2[-1+i]+kk7+dxx
        koordinat2.append(xx4)
    for k in range(kolvo8):
        i=i+1
        if k==0:
            xx5=koordinat2[-1+i]+kk7/2+kk8/2+dxx #при первом вводе
        else:
            xx5=koordinat2[-1+i]+kk8+dxx
        koordinat2.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat2)):
        if koordinat2[x]<x2:
            koordinat2[x]=koordinat2[x]-ror
        else:
            koordinat2[x]=koordinat2[x]+ror

    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et):
        for k in range(kolvo):
            i=i+1
            #ввод
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0.0
            iPlacementParam.scale_ = 1.0
            iPlacementParam.yBase = koordinat2[-1+i]
            iPlacementParam.xBase = x2
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.yBase = koordinat2[-1+i]
                iPlacementParam.xBase = x2
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #проекции
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            if shir==4:
                obj = iFragment.ksFragmentDefinition(pro1,pro1,0)
            if shir==6:
                obj = iFragment.ksFragmentDefinition(pro2,pro2,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = -90.0
            iPlacementParam.scale_ = 1.0
            iPlacementParam.yBase = koordinat2[-1+i]
            iPlacementParam.xBase = x2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k==0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.yBase = koordinat2[-1+i]
                iPlacementParam.xBase = x2
                iFragment.ksReadFragment(pos, True, iPlacementParam)

    Vvod_input(i=0,kolvo=kolvo5,a=a5,pro1=pro51,pro2=pro52,pos=pos5,et=et5)
    Vvod_input(i=kolvo5,kolvo=kolvo6,a=a6,pro1=pro61,pro2=pro62,pos=pos6,et=et6)
    Vvod_input(i=kolvo5+kolvo6,kolvo=kolvo7,a=a7,pro1=pro71,pro2=pro72,pos=pos7,et=et7)
    Vvod_input(i=kolvo5+kolvo6+kolvo7,kolvo=kolvo8,a=a8,pro1=pro81,pro2=pro82,pos=pos8,et=et8)
    
if dL<=0:
    #расчет координат
    if e5==2:
        kk5=kk5*1.2
    if e6==2:
        kk6=kk6*1.2
    if e7==2:
        kk7=kk7*1.2
    if e8==2:
        kk8=kk8*1.2
    
