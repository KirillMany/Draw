ror=0
if x1 == u'CM A139'or x1 == u'BM A139':
    ror=28

for K17, v17, in dict_vvod_data1.items():
    if K17==vvod17:
        a17=v17[0]
        dx17=v17[1]
        kk17=v17[2]

        Y17=v17[4]
        K17=v17[3]
        e17=v17[5]

        pos17=v17[6]
        pro171=v17[7]
        pro52=v17[8]

        et17=v17[9]
        
    
for K18, v18, in dict_vvod_data1.items(): 
    if K18==vvod18:
        a18=v18[0]
        dx18=v18[1]
        kk18=v18[2]
        
        
        Y18=v18[4]
        K18=v18[3]
        e18=v18[5]

        pos18=v18[6]
        pro181=v18[7]
        pro62=v18[8]

        et18=v18[9]
        
for K19, v19, in dict_vvod_data1.items(): 
    if K19==vvod19:
        a19=v19[0]
        dx19=v19[1]
        kk19=v19[2]

        Y19=v19[4]
        K19=v19[3]
        e19=v19[5]

        pos19=v19[6]
        pro191=v19[7]
        pro72=v19[8]

        et19=v19[9]

for K20, v20, in dict_vvod_data1.items(): 
    if K20==vvod20:
        a20=v20[0]
        dx20=v20[1]
        kk20=v20[2]

        Y20=v20[4]
        K20=v20[3]
        e20=v20[5]

        pos20=v20[6]
        pro201=v20[7]
        pro82=v20[8]

        et20=v20[9]
            
dlina1=kk17*kolvo17+kk18*kolvo18+kk19*kolvo19+kk20*kolvo20
dL=dlina-dlina1
summa=kolvo17+kolvo18+kolvo19+kolvo20#сумма всех вводов
dxx=dL/(summa+1)#отсуп

if dL>0:
    koordinat4=[]

    if kolvo17==0:
        kk17=kk18      
    if kolvo18==0:
        kk18=kk17
    if kolvo19==0:
        kk19=kk18
    if kolvo20==0:
        kk20=kk19
    xx1=y2-dlina/2+dxx+kk17/2#расчет первой координаты    
    koordinat4.append(xx1)
    i=0
    for k in range(kolvo17-1):
        i=i+1
        xx2=koordinat4[-1+i]+kk17+dxx
        koordinat4.append(xx2)
    for k in range(kolvo18):
        i=i+1
        if k==0:
            xx3=koordinat4[-1+i]+kk17/2+kk18/2+dxx #при первом вводе
        else:
            xx3=koordinat4[-1+i]+kk18+dxx
        koordinat4.append(xx3)
    for k in range(kolvo19):
        i=i+1
        if k==0:
            xx4=koordinat4[-1+i]+kk18/2+kk19/2+dxx #при первом вводе
        else:
            xx4=koordinat4[-1+i]+kk19+dxx
        koordinat4.append(xx4)
    for k in range(kolvo20):
        i=i+1
        if k==0:
            xx5=koordinat4[-1+i]+kk19/2+kk20/2+dxx #при первом вводе
        else:
            xx5=koordinat4[-1+i]+kk20+dxx
        koordinat4.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat4)):
        if koordinat4[x]<x2:
            koordinat4[x]=koordinat4[x]-ror
        else:
            koordinat4[x]=koordinat4[x]+ror

    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et):
        for k in range(kolvo):
            i=i+1
            #ввод
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0.0
            iPlacementParam.scale_ = 1.0
            iPlacementParam.yBase = koordinat4[-1+i]
            iPlacementParam.xBase = x2
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.yBase = koordinat4[-1+i]
                iPlacementParam.xBase = x2
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #проекции
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            if shir==4:
                obj = iFragment.ksFragmentDefinition(pro1,pro1,0)
            if shir==6:
                obj = iFragment.ksFragmentDefinition(pro2,pro2,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 90.0
            iPlacementParam.scale_ = 1.0
            iPlacementParam.yBase = koordinat4[-1+i]
            iPlacementParam.xBase = x2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k==0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.yBase = koordinat4[-1+i]
                iPlacementParam.xBase = x2
                iFragment.ksReadFragment(pos, True, iPlacementParam)

    Vvod_input(i=0,kolvo=kolvo17,a=a17,pro1=pro171,pro2=pro52,pos=pos17,et=et17)
    Vvod_input(i=kolvo17,kolvo=kolvo18,a=a18,pro1=pro181,pro2=pro62,pos=pos18,et=et18)
    Vvod_input(i=kolvo17+kolvo18,kolvo=kolvo19,a=a19,pro1=pro191,pro2=pro72,pos=pos19,et=et19)
    Vvod_input(i=kolvo17+kolvo18+kolvo19,kolvo=kolvo20,a=a20,pro1=pro201,pro2=pro82,pos=pos20,et=et20)
    
if dL<=0:
    #расчет координат
    if e17==2:
        kk17=kk17*1.2
    if e18==2:
        kk18=kk18*1.2
    if e19==2:
        kk19=kk19*1.2
    if e20==2:
        kk20=kk20*1.2
    
    dlina1=kk17*kolvo17+kk18*kolvo18+kk19*kolvo19+kk20*kolvo20
    dlina=dlina*0.9
    
    dL=dlina-dlina1
    summa=kolvo17+kolvo18+kolvo19+kolvo20#сумма всех вводов
    dxx=dL/(summa+1)#отсуп
    koordinat4=[]
        
    if kolvo17==0:
        kk17=kk18      
    if kolvo18==0:
        kk18=kk17
    if kolvo19==0:
        kk19=kk18
    if kolvo20==0:
        kk20=kk19
    xx1=x2-dlina/2+dxx+kk17/2#расчет первой координаты    
    koordinat4.append(xx1)
    i=0
    for k in range(kolvo17-1):
        i=i+1
        xx2=koordinat4[-1+i]+kk17+dxx
        koordinat4.append(xx2)
    for k in range(kolvo18):
        i=i+1
        if k==0:
            xx3=koordinat4[-1+i]+kk17/2+kk18/2+dxx #при первом вводе
        else:
            xx3=koordinat4[-1+i]+kk18+dxx
        koordinat4.append(xx3)
    for k in range(kolvo19):
        i=i+1
        if k==0:
            xx4=koordinat4[-1+i]+kk18/2+kk19/2+dxx #при первом вводе
        else:
            xx4=koordinat4[-1+i]+kk19+dxx
        koordinat4.append(xx4)
    for k in range(kolvo20):
        i=i+1
        if k==0:
            xx5=koordinat4[-1+i]+kk19/2+kk20/2+dxx #при первом вводе
        else:
            xx5=koordinat4[-1+i]+kk20+dxx
        koordinat4.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat4)):
        if koordinat4[x]<x2:
            koordinat4[x]=koordinat4[x]-ror
        else:
            koordinat4[x]=koordinat4[x]+ror
    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et,Y):
        for k in range(kolvo):
            #ввод
            i=i+1
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0.0
            iPlacementParam.scale_ = 1.0
            iPlacementParam.yBase = koordinat4[-1+i]
            if i%2==0:
                iPlacementParam.xBase = x2+Y
            if i%2==1:
                iPlacementParam.xBase = x2-Y
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.yBase = koordinat4[-1+i]
                if i%2==0:
                    iPlacementParam.xBase = x2+Y
                if i%2==1:
                    iPlacementParam.xBase = x2-Y
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #проекции
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            if shir==4:
                obj = iFragment.ksFragmentDefinition(pro1,pro1,0)
            if shir==6:
                obj = iFragment.ksFragmentDefinition(pro2,pro2,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 90.0
            iPlacementParam.scale_ = 1.0
            iPlacementParam.yBase = koordinat4[-1+i]
            iPlacementParam.xBase = x2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k==0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.yBase = koordinat4[-1+i]
                if i%2==0:
                    iPlacementParam.xBase = x2+Y
                if i%2==1:
                    iPlacementParam.xBase = x2-Y
                iFragment.ksReadFragment(pos, True, iPlacementParam)

    Vvod_input(i=0,kolvo=kolvo17,a=a17,pro1=pro171,pro2=pro52,pos=pos17,et=et17,Y=Y17)
    Vvod_input(i=kolvo17,kolvo=kolvo18,a=a18,pro1=pro181,pro2=pro62,pos=pos18,et=et18,Y=Y18)
    Vvod_input(i=kolvo17+kolvo18,kolvo=kolvo19,a=a19,pro1=pro191,pro2=pro72,pos=pos19,et=et19,Y=Y19)
    Vvod_input(i=kolvo17+kolvo18+kolvo19,kolvo=kolvo20,a=a20,pro1=pro201,pro2=pro82,pos=pos20,et=et20,Y=Y20)










