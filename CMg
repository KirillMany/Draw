a1=0
a2=0
a3=0
a4=0

dx1=0
dx2=0
dx3=0
dx4=0

kk1=0
kk2=0
kk3=0
kk4=0

Y1=0
Y2=0
Y3=0
Y4=0

K1=0
K2=0
K3=0
K4=0

e1=0
e2=0
e3=0
e4=0

pos1=0
pos2=0
pos3=0
pos4=0

pro11=0
pro21=0
pro31=0
pro41=0

