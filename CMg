def InsertZps(v):
    iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
    obj = iFragment.ksFragmentDefinition(v, v, 0)
    iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
    iPlacementParam.Init()
    iPlacementParam.angle = 1
    iPlacementParam.scale_ = 1.0
    iPlacementParam.xBase = 15.0
    iPlacementParam.yBase = 15.0
    iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)

    
#пройдемся по словарю
for k, v in dict_insert_zip.items():
    for t in range(23):
        if k==sheet3.Cells(32+t,43).value:
            for i in range (sheet3.Cells(32+t,11)):
                InsertZps(v)
           

#создадим папку с названием файла
#определим ячейки для ввода пути к папке
a = u'U:\\Peppers\\10_Проектирование\\~АрхивЧертежей~\\Архив УУМ'
#основной путь
sheet4 = app.ActiveWorkbook.Sheets(u'Главная')

b = (sheet4.Cells(47,67).value)
#серия W или 1
c = (sheet4.Cells(48,67).value)
#серия TM или ВМ
d = (sheet4.Cells(46,67).value)
#материал полиэстер или алюминий
e = (sheet4.Cells(44,67).value)
#номер корпуса
ee=u'\\'+c+u' '+d+b+e
#определим крайнюю папку
app.ActiveWindow.Close(True)
app.Quit()
#опишем условия создания папок
if b == u'W':
    b=u'\\Серия W'
if b == u'1':
    b=u'\\Серия 100'
if c == u'CM':
    c=u'\\CM'
    if d == u'P':
        d=u'\\CM P'
    if d == u'A':
        d=u'\\CM A'
f=nameis
#имя файла
g=a+b+c+d+ee+u'\\'+f
#генерация полного пути

try:
    os.mkdir(g)
    #создание папки
except WindowsError:
    tkMessageBox.showinfo(title=u"Прошу проверить!", message=u"Такая папка уже есть!")
    sys.exit()

#сохраним чертеж в папку
k1=u'/'+ nameis + u' ВО.cdw'
#для чертежа ОВ
k2=u'/'+ nameis + u'.spw'

f1=g+k1   
f2=g+k2

iDocument2D.ksSaveDocument(f1)
#сохраним ОВ

iDocument2D.ksCloseDocument()
#закроем ОВ

#блок для спецификации

iDocumentSpc = iKompasObject.SpcDocument()
iDocumentParam = KAPI.ksDocumentParam(iKompasObject.GetParamStruct(LDefin2D.ko_DocumentParam))
iDocumentParam.Init()
iDocumentParam.type = 4

iSheetParam = KAPI.ksSheetPar(iDocumentParam.GetLayoutParam())
iSheetParam.Init()
iSheetParam.layoutName = "C:\Program Files\ASCON\KOMPAS-3D V16\Sys\GRAPHIC.LYT"
iSheetParam.shtType = 1

iDocumentSpc.ksCreateDocument(iDocumentParam)
#создаем спеку
iDocument = iApplication.ActiveDocument


#подключаем к документу

from win32com.client import Dispatch, gencache
#  Получи API интерфейсов версии 5
kompas6_api5_module = gencache.EnsureModule("{0422828C-F174-495E-AC5D-D31014DBBE87}", 0, 1, 0)
kompas_object = kompas6_api5_module.KompasObject(
    Dispatch("Kompas.Application.5")._oleobj_.QueryInterface(kompas6_api5_module.KompasObject.CLSID,
                                                             pythoncom.IID_IDispatch))

#  Получи API интерфейсов версии 7
kompas_api7_module = gencache.EnsureModule("{69AC2981-37C0-4379-84FD-5DD2F3C0A520}", 0, 1, 0)
kompas_api_object = kompas_api7_module.IKompasAPIObject(
    Dispatch("Kompas.Application.7")._oleobj_.QueryInterface(kompas_api7_module.IKompasAPIObject.CLSID,
                                                             pythoncom.IID_IDispatch))
application = kompas_api_object.Application

iSpecificationDocument = iDocument._oleobj_.QueryInterface(
    kompas_api7_module.NamesToIIDMap['ISpecificationDocument'], pythoncom.IID_IDispatch)
iSpecificationDocument = kompas_api7_module.ISpecificationDocument(iSpecificationDocument)
AttachedDocuments = iSpecificationDocument.AttachedDocuments
openfilename1=f1
iAttachedDocument = AttachedDocuments.Add(r"" + openfilename1, True)

Name = iAttachedDocument.Name
openfilename2=f2    
iDocument.SaveAs("" + openfilename2)
iDocumentSpc.ksCloseDocument()
iDocumentSpc.ksOpenDocument("" + openfilename2, False)

