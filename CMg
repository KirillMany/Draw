if drenazh == u'да ТГК':
    if kolvo12==0:
        a12=box_vvod[756]
        dx12=33
        kk12=36.6

        Y12=yy0
        K12=kk00
        e12=1

        pos12=box_vvod[760]
        pro121=box_vvod[758]
        pro122=box_vvod[759]

        et12=box_vvod[744]
        kolvo12=1
    
dlina1=kk9*kolvo9+kk10*kolvo10+kk11*kolvo11+kk12*kolvo12+kk13*kolvo13+kk14*kolvo14+kk15*kolvo15+kk16*kolvo16
dL=dlina-dlina1
summa=kolvo9+kolvo10+kolvo11+kolvo12+kolvo13+kolvo14+kolvo15+kolvo16#сумма всех вводов
dxx=dL/(summa+1)#отсуп

if dL>0:
    koordinat3=[]

    if kolvo9==0:
        kk9=kk10      
    if kolvo10==0:
        kk10=kk9
    if kolvo11==0:
        kk11=kk10
    if kolvo12==0:
        kk12=kk11
                
    xx1=x2-dlina/2+dxx+kk9/2#расчет первой координаты    
    koordinat3.append(xx1)
    i=0
    for k in range(kolvo9-1):
        i=i+1
        xx2=koordinat3[-1+i]+kk9+dxx
        koordinat3.append(xx2)
    for k in range(kolvo10):
        i=i+1
        if k==0:
            xx3=koordinat3[-1+i]+kk9/2+kk10/2+dxx #при первом вводе
        else:
            xx3=koordinat3[-1+i]+kk10+dxx
        koordinat3.append(xx3)
    for k in range(kolvo11):
        i=i+1
        if k==0:
            xx4=koordinat3[-1+i]+kk10/2+kk11/2+dxx #при первом вводе
        else:
            xx4=koordinat3[-1+i]+kk11+dxx
        koordinat3.append(xx4)
    for k in range(kolvo12):
        i=i+1
        if k==0:
            xx5=koordinat3[-1+i]+kk11/2+kk12/2+dxx #при первом вводе
        else:
            xx5=koordinat3[-1+i]+kk12+dxx
        koordinat3.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat3)):
        if koordinat3[x]<x2:
            koordinat3[x]=koordinat3[x]-ror
        else:
            koordinat3[x]=koordinat3[x]+ror
    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et):
        for k in range(kolvo):
            i=i+1
            #ввод
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0.0
            iPlacementParam.scale_ = 1.0
            iPlacementParam.xBase = koordinat3[-1+i]
            iPlacementParam.yBase = y2
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.xBase = koordinat3[-1+i]
                iPlacementParam.yBase = y2
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #проекции
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            if shir==4:
                obj = iFragment.ksFragmentDefinition(pro1,pro1,0)
            if shir==6:
                obj = iFragment.ksFragmentDefinition(pro2,pro2,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 180
            iPlacementParam.scale_ = 1.0
            iPlacementParam.xBase = koordinat3[-1+i]
            iPlacementParam.yBase = y2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k==0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.xBase = koordinat3[-1+i]
                iPlacementParam.yBase = y2
                iFragment.ksReadFragment(pos, True, iPlacementParam)

    Vvod_input(i=0,kolvo=kolvo9,a=a9,pro1=pro91,pro2=pro92,pos=pos9,et=et9)
    Vvod_input(i=kolvo9,kolvo=kolvo10,a=a10,pro1=pro101,pro2=pro102,pos=pos10,et=et10)
    Vvod_input(i=kolvo9+kolvo10,kolvo=kolvo11,a=a11,pro1=pro111,pro2=pro112,pos=pos11,et=et11)
    Vvod_input(i=kolvo9+kolvo10+kolvo11,kolvo=kolvo12,a=a12,pro1=pro121,pro2=pro122,pos=pos12,et=et12)


if dL<=0:
    #расчет координат
    if e9==2:
        kk9=kk9*1.2
    if e10==2:
        kk10=kk10*1.2
    if e11==2:
        kk11=kk11*1.2
    if e12==2:
        kk12=kk12*1.2
    
    dlina1=kk9*kolvo9+kk10*kolvo10+kk11*kolvo11+kk12*kolvo12
    dlina=dlina*0.9
    
    dL=dlina-dlina1
    summa=kolvo9+kolvo10+kolvo11+kolvo12#сумма всех вводов
    dxx=dL/(summa+1)#отсуп
    koordinat3=[]
        
    if kolvo9==0:
        kk9=kk10      
    if kolvo10==0:
        kk10=kk9
    if kolvo11==0:
        kk11=kk10
    if kolvo12==0:
        kk12=kk11
    xx1=x2-dlina/2+dxx+kk9/2#расчет первой координаты    
    koordinat3.append(xx1)
    i=0
    for k in range(kolvo9-1):
        i=i+1
        xx2=koordinat3[-1+i]+kk9+dxx
        koordinat3.append(xx2)
    for k in range(kolvo10):
        i=i+1
        if k==0:
            xx3=koordinat3[-1+i]+kk9/2+kk10/2+dxx #при первом вводе
        else:
            xx3=koordinat3[-1+i]+kk10+dxx
        koordinat3.append(xx3)
    for k in range(kolvo11):
        i=i+1
        if k==0:
            xx4=koordinat3[-1+i]+kk10/2+kk11/2+dxx #при первом вводе
        else:
            xx4=koordinat3[-1+i]+kk11+dxx
        koordinat3.append(xx4)
    for k in range(kolvo12):
        i=i+1
        if k==0:
            xx5=koordinat3[-1+i]+kk11/2+kk12/2+dxx #при первом вводе
        else:
            xx5=koordinat3[-1+i]+kk12+dxx
        koordinat3.append(xx5)
    #напишем блок для смещения для длинных корпусов
    for x in range(len(koordinat3)):
        if koordinat3[x]<x2:
            koordinat3[x]=koordinat3[x]-ror
        else:
            koordinat3[x]=koordinat3[x]+ror
    
    def Vvod_input(i,kolvo,a,pro1,pro2,pos,et,Y):
        for k in range(kolvo):
            #ввод
            i=i+1
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            obj = iFragment.ksFragmentDefinition(a,a,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 0.0
            iPlacementParam.scale_ = 1.0
            iPlacementParam.xBase = koordinat3[-1+i]
            if i%2==0:
                iPlacementParam.yBase = y2+Y
            if i%2==1:
                iPlacementParam.yBase = y2-Y
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            if kz==u'да':
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                obj = iFragment.ksFragmentDefinition(et,et,0)
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.xBase = koordinat3[-1+i]
                if i%2==0:
                    iPlacementParam.yBase = y2+Y
                if i%2==1:
                    iPlacementParam.yBase = y2-Y
                iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #проекции
            iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
            if shir==4:
                obj = iFragment.ksFragmentDefinition(pro1,pro1,0)
            if shir==6:
                obj = iFragment.ksFragmentDefinition(pro2,pro2,0)
            iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
            iPlacementParam.Init()
            iPlacementParam.angle = 180
            iPlacementParam.scale_ = 1.0
            iPlacementParam.xBase = koordinat3[-1+i]
            iPlacementParam.yBase = y2+y22
            iFragment.ksInsertFragmentEx(obj, False, iPlacementParam, 1)
            #вставим позиции
            if k==0:
                iFragment = KAPI.ksFragment(iDocument2D.GetFragment())
                iPlacementParam = KAPI.ksPlacementParam(iKompasObject.GetParamStruct(LDefin2D.ko_PlacementParam))
                iPlacementParam.Init()
                iPlacementParam.angle = 0.0
                iPlacementParam.scale_ = 1.0
                iPlacementParam.xBase = koordinat3[-1+i]
                if i%2==0:
                    iPlacementParam.yBase = y2+Y
                if i%2==1:
                    iPlacementParam.yBase = y2-Y
                iFragment.ksReadFragment(pos, True, iPlacementParam)

    Vvod_input(i=0,kolvo=kolvo9,a=a9,pro1=pro91,pro2=pro92,pos=pos9,et=et9,Y=Y9)
    Vvod_input(i=kolvo9,kolvo=kolvo10,a=a10,pro1=pro101,pro2=pro102,pos=pos10,et=et10,Y=Y10)
    Vvod_input(i=kolvo9+kolvo10,kolvo=kolvo11,a=a11,pro1=pro111,pro2=pro112,pos=pos11,et=et11,Y=Y11)
    Vvod_input(i=kolvo9+kolvo10+kolvo11,kolvo=kolvo12,a=a12,pro1=pro121,pro2=pro122,pos=pos12,et=et12,Y=Y12)


